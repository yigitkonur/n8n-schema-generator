{
  "name": "n8n-schema-generator",
  "version": "1.0.0",
  "description": "Auto-updating JSON schemas for every n8n node + validation API",
  "type": "module",
  "scripts": {
    "extract": "npx tsc && node dist/extract.js",
    "extract:force": "FORCE_UPDATE=1 npm run extract",
    "serve": "npx tsc && node dist/server.js",
    "start": "node dist/server.js"
  },
  "keywords": [
    "n8n",
    "schema",
    "json",
    "automation",
    "mcp",
    "ai",
    "validation",
    "api"
  ],
  "author": "Yigit Konur",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "n8n-nodes-base": "^1.120.1",
    "n8n-workflow": "^1.119.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
