{
  "name": "jotFormTrigger",
  "displayName": "Jotform Trigger",
  "description": "Handle Jotform events via webhooks",
  "group": [
    "trigger"
  ],
  "version": 1,
  "defaults": {
    "form": "",
    "resolveData": true,
    "onlyAnswers": true
  },
  "inputs": [],
  "outputs": [
    "main"
  ],
  "properties": [
    {
      "name": "form",
      "displayName": "Form Name or ID",
      "type": "options",
      "default": "",
      "required": true,
      "description": "Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>",
      "typeOptions": {
        "loadOptionsMethod": "getForms"
      }
    },
    {
      "name": "resolveData",
      "displayName": "Resolve Data",
      "type": "boolean",
      "default": true,
      "description": "By default does the webhook-data use internal keys instead of the names. If this option gets activated, it will resolve the keys automatically to the actual names."
    },
    {
      "name": "onlyAnswers",
      "displayName": "Only Answers",
      "type": "boolean",
      "default": true,
      "description": "Whether to return only the answers of the form and not any of the other data"
    }
  ],
  "credentials": [
    {
      "name": "jotFormApi",
      "required": true
    }
  ],
  "requiredFields": [
    "form"
  ],
  "optionalFields": [
    "resolveData",
    "onlyAnswers"
  ],
  "parameterTypes": {
    "form": "options",
    "resolveData": "boolean",
    "onlyAnswers": "boolean"
  },
  "hasVersions": false
}