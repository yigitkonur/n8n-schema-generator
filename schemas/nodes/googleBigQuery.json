      "type": "options",
      "default": "executeQuery",
      "options": [
        {
          "name": "Execute Query",
          "value": "executeQuery",
          "description": "Execute a SQL query",
          "action": "Execute a SQL query"
        },
        {
          "name": "Insert",
          "value": "insert",
          "description": "Insert rows in a table",
          "action": "Insert rows in a table"
        }
