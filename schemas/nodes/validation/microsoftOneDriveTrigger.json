{
  "nodeType": "n8n-nodes-base.microsoftOneDriveTrigger",
  "displayName": "Microsoft OneDrive Trigger",
  "version": 1,
  "rules": [
    {
      "field": "event",
      "type": "options",
      "required": false,
      "default": "fileCreated",
      "enumValues": [
        "fileCreated",
        "fileUpdated",
        "folderCreated",
        "folderUpdated"
      ]
    },
    {
      "field": "simple",
      "type": "boolean",
      "required": false,
      "default": true
    },
    {
      "field": "watchFolder",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "event": [
            "fileCreated"
          ]
        }
      }
    },
    {
      "field": "watch",
      "type": "options",
      "required": false,
      "default": "anyFile",
      "enumValues": [
        "anyFile",
        "selectedFolder",
        "selectedFile"
      ],
      "displayOptions": {
        "show": {
          "event": [
            "fileUpdated"
          ]
        }
      }
    },
    {
      "field": "watchFolder",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "event": [
            "folderCreated"
          ]
        }
      }
    },
    {
      "field": "watch",
      "type": "options",
      "required": false,
      "default": "anyFolder",
      "enumValues": [
        "anyFolder",
        "selectedFolder",
        "oneSelectedFolder"
      ],
      "displayOptions": {
        "show": {
          "event": [
            "folderUpdated"
          ]
        }
      }
    },
    {
      "field": "fileId",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "id",
        "value": ""
      },
      "displayOptions": {
        "show": {
          "event": [
            "fileUpdated"
          ],
          "watch": [
            "selectedFile"
          ]
        }
      }
    },
    {
      "field": "folderId",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "id",
        "value": "",
        "cachedResultName": ""
      },
      "displayOptions": {
        "show": {
          "watch": [
            "selectedFolder",
            "oneSelectedFolder"
          ]
        }
      }
    },
    {
      "field": "folderId",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "id",
        "value": "",
        "cachedResultName": ""
      },
      "displayOptions": {
        "show": {
          "watchFolder": [
            true
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "watch": [
            "selectedFolder"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "watchFolder": [
            true
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "event": "fileCreated",
    "simple": true,
    "watchFolder": false,
    "fileId": {
      "__rl": true,
      "mode": "id",
      "value": ""
    },
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}