{
  "nodeType": "n8n-nodes-base.netlifyTrigger",
  "displayName": "Netlify Trigger",
  "version": 1,
  "rules": [
    {
      "field": "siteId",
      "type": "options",
      "required": true,
      "default": ""
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "default": "",
      "enumValues": [
        "deployBuilding",
        "deployFailed",
        "deployCreated",
        "submissionCreated"
      ]
    },
    {
      "field": "formId",
      "type": "options",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "event": [
            "submissionCreated"
          ]
        }
      }
    },
    {
      "field": "simple",
      "type": "boolean",
      "required": false,
      "default": true,
      "displayOptions": {
        "show": {
          "event": [
            "submissionCreated"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "siteId": "",
    "event": "",
    "formId": "",
    "simple": true
  },
  "hasFilter": false,
  "fixedCollections": []
}