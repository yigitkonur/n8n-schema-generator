{
  "nodeType": "n8n-nodes-base.acuitySchedulingTrigger",
  "displayName": "Acuity Scheduling Trigger",
  "version": 1,
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "apiKey",
      "enumValues": [
        "apiKey",
        "oAuth2"
      ]
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "default": "",
      "enumValues": [
        "appointment.canceled",
        "appointment.changed",
        "appointment.rescheduled",
        "appointment.scheduled",
        "order.completed"
      ]
    },
    {
      "field": "resolveData",
      "type": "boolean",
      "required": false,
      "default": true
    }
  ],
  "computedDefaults": {
    "authentication": "apiKey",
    "event": "",
    "resolveData": true
  },
  "hasFilter": false,
  "fixedCollections": []
}