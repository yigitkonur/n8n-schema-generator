{
  "nodeType": "n8n-nodes-base.jwt",
  "displayName": "JWT",
  "version": 1,
  "rules": [
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "sign",
      "enumValues": [
        "decode",
        "sign",
        "verify"
      ]
    },
    {
      "field": "useJson",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "operation": [
            "sign"
          ]
        }
      }
    },
    {
      "field": "claims",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "operation": [
            "sign"
          ],
          "useJson": [
            false
          ]
        }
      }
    },
    {
      "field": "claimsJson",
      "type": "json",
      "required": false,
      "default": "{\n  \"my_field_1\": \"value 1\",\n  \"my_field_2\": \"value 2\"\n}\n",
      "displayOptions": {
        "show": {
          "operation": [
            "sign"
          ],
          "useJson": [
            true
          ]
        }
      }
    },
    {
      "field": "token",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "verify",
            "decode"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "operation": "sign",
    "useJson": false,
    "claims": {},
    "claimsJson": "{\n  \"my_field_1\": \"value 1\",\n  \"my_field_2\": \"value 2\"\n}\n",
    "token": "",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}