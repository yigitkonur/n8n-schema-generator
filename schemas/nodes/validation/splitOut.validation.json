{
  "nodeType": "n8n-nodes-base.splitOut",
  "displayName": "Split Out",
  "version": 1,
  "category": [
    "transform"
  ],
  "requiredParameters": [
    "fieldToSplitOut"
  ],
  "optionalParameters": [
    "include",
    "fieldsToInclude",
    "options"
  ],
  "validationRules": [
    {
      "field": "fieldToSplitOut",
      "type": "string",
      "required": true,
      "constraints": {
        "type": "string",
        "default": ""
      }
    },
    {
      "field": "include",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "noOtherFields",
        "enumValues": [
          "noOtherFields",
          "allOtherFields",
          "selectedOtherFields"
        ]
      }
    },
    {
      "field": "fieldsToInclude",
      "type": "string",
      "required": false,
      "constraints": {
        "type": "string",
        "default": ""
      },
      "displayOptions": {
        "show": {
          "include": [
            "selectedOtherFields"
          ]
        }
      },
      "dependsOn": [
        "include"
      ]
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "constraints": {
        "type": "collection",
        "default": {},
        "properties": {},
        "requiredProperties": []
      }
    }
  ],
  "computedDefaults": {
    "fieldToSplitOut": "",
    "include": "noOtherFields",
    "fieldsToInclude": "",
    "options": {}
  },
  "conditionalFields": [
    {
      "field": "fieldsToInclude",
      "condition": {
        "show": {
          "include": [
            "selectedOtherFields"
          ]
        }
      },
      "dependsOnFields": [
        "include"
      ]
    }
  ],
  "fixedCollections": []
}