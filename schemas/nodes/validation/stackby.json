{
  "nodeType": "n8n-nodes-base.stackby",
  "displayName": "Stackby",
  "version": 1,
  "rules": [
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "append",
      "enumValues": [
        "append",
        "delete",
        "list",
        "read"
      ]
    },
    {
      "field": "stackId",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "table",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "id",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "read",
            "delete"
          ]
        }
      }
    },
    {
      "field": "returnAll",
      "type": "boolean",
      "required": false,
      "default": true,
      "displayOptions": {
        "show": {
          "operation": [
            "list"
          ]
        }
      }
    },
    {
      "field": "limit",
      "type": "number",
      "required": false,
      "default": 1000,
      "displayOptions": {
        "show": {
          "operation": [
            "list"
          ],
          "returnAll": [
            false
          ]
        }
      }
    },
    {
      "field": "additionalFields",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "operation": [
            "list"
          ]
        }
      }
    },
    {
      "field": "columns",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "append"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "operation": "append",
    "stackId": "",
    "table": "",
    "id": "",
    "returnAll": true,
    "limit": 1000,
    "additionalFields": {},
    "columns": ""
  },
  "hasFilter": false,
  "fixedCollections": []
}