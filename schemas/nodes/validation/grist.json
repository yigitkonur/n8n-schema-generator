{
  "nodeType": "n8n-nodes-base.grist",
  "displayName": "Grist",
  "version": 1,
  "rules": [
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "getAll",
      "enumValues": [
        "create",
        "delete",
        "getAll",
        "update"
      ]
    },
    {
      "field": "docId",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "tableId",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "rowId",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "delete"
          ]
        }
      }
    },
    {
      "field": "returnAll",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ]
        }
      }
    },
    {
      "field": "limit",
      "type": "number",
      "required": false,
      "default": 50,
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ],
          "returnAll": [
            false
          ]
        }
      }
    },
    {
      "field": "additionalOptions",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ]
        }
      }
    },
    {
      "field": "rowId",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "update"
          ]
        }
      }
    },
    {
      "field": "dataToSend",
      "type": "options",
      "required": false,
      "default": "defineInNode",
      "enumValues": [
        "autoMapInputs",
        "defineInNode"
      ],
      "displayOptions": {
        "show": {
          "operation": [
            "create",
            "update"
          ]
        }
      }
    },
    {
      "field": "inputsToIgnore",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "create",
            "update"
          ],
          "dataToSend": [
            "autoMapInputs"
          ]
        }
      }
    },
    {
      "field": "fieldsToSend",
      "type": "fixedCollection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "operation": [
            "create",
            "update"
          ],
          "dataToSend": [
            "defineInNode"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "operation": "getAll",
    "docId": "",
    "tableId": "",
    "returnAll": false,
    "limit": 50,
    "additionalOptions": {},
    "dataToSend": "defineInNode",
    "inputsToIgnore": "",
    "fieldsToSend": {}
  },
  "hasFilter": false,
  "fixedCollections": [
    "fieldsToSend"
  ]
}