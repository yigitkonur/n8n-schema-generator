{
  "nodeType": "n8n-nodes-base.baserow",
  "displayName": "Baserow",
  "version": 1,
  "rules": [
    {
      "field": "resource",
      "type": "options",
      "required": false,
      "default": "row",
      "enumValues": [
        "row"
      ]
    },
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "getAll",
      "enumValues": [
        "create",
        "delete",
        "get",
        "getAll",
        "update"
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "row"
          ]
        }
      }
    },
    {
      "field": "databaseId",
      "type": "options",
      "required": true,
      "default": ""
    },
    {
      "field": "tableId",
      "type": "options",
      "required": true,
      "default": ""
    },
    {
      "field": "rowId",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "get"
          ]
        }
      }
    },
    {
      "field": "rowId",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "update"
          ]
        }
      }
    },
    {
      "field": "dataToSend",
      "type": "options",
      "required": false,
      "default": "defineBelow",
      "enumValues": [
        "autoMapInputData",
        "defineBelow"
      ],
      "displayOptions": {
        "show": {
          "operation": [
            "create",
            "update"
          ]
        }
      }
    },
    {
      "field": "inputsToIgnore",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "create",
            "update"
          ],
          "dataToSend": [
            "autoMapInputData"
          ]
        }
      }
    },
    {
      "field": "fieldsUi",
      "type": "fixedCollection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "operation": [
            "create",
            "update"
          ],
          "dataToSend": [
            "defineBelow"
          ]
        }
      }
    },
    {
      "field": "rowId",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "delete"
          ]
        }
      }
    },
    {
      "field": "returnAll",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ]
        }
      }
    },
    {
      "field": "limit",
      "type": "number",
      "required": false,
      "default": 50,
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ],
          "returnAll": [
            false
          ]
        }
      }
    },
    {
      "field": "additionalOptions",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "resource": "row",
    "operation": "getAll",
    "databaseId": "",
    "tableId": "",
    "dataToSend": "defineBelow",
    "inputsToIgnore": "",
    "fieldsUi": {},
    "returnAll": false,
    "limit": 50,
    "additionalOptions": {}
  },
  "hasFilter": false,
  "fixedCollections": [
    "fieldsUi"
  ]
}