{
  "nodeType": "n8n-nodes-base.npm",
  "displayName": "Npm",
  "version": 1,
  "rules": [
    {
      "field": "resource",
      "type": "options",
      "required": false,
      "default": "package",
      "enumValues": [
        "package",
        "distTag"
      ]
    },
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "getMetadata",
      "enumValues": [
        "getMetadata",
        "getVersions",
        "search"
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "package"
          ]
        }
      }
    },
    {
      "field": "packageName",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "resource": [
            "package"
          ],
          "operation": [
            "getMetadata",
            "getVersions"
          ]
        }
      }
    },
    {
      "field": "packageVersion",
      "type": "string",
      "required": true,
      "default": "latest",
      "displayOptions": {
        "show": {
          "resource": [
            "package"
          ],
          "operation": [
            "getMetadata"
          ]
        }
      }
    },
    {
      "field": "query",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "resource": [
            "package"
          ],
          "operation": [
            "search"
          ]
        }
      }
    },
    {
      "field": "limit",
      "type": "number",
      "required": false,
      "default": 10,
      "displayOptions": {
        "show": {
          "resource": [
            "package"
          ],
          "operation": [
            "search"
          ]
        }
      }
    },
    {
      "field": "offset",
      "type": "number",
      "required": false,
      "default": 0,
      "displayOptions": {
        "show": {
          "resource": [
            "package"
          ],
          "operation": [
            "search"
          ]
        }
      }
    },
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "getMany",
      "enumValues": [
        "getMany",
        "update"
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "distTag"
          ]
        }
      }
    },
    {
      "field": "packageName",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "resource": [
            "distTag"
          ],
          "operation": [
            "getMany",
            "update"
          ]
        }
      }
    },
    {
      "field": "packageVersion",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "resource": [
            "distTag"
          ],
          "operation": [
            "update"
          ]
        }
      }
    },
    {
      "field": "distTagName",
      "type": "string",
      "required": true,
      "default": "latest",
      "displayOptions": {
        "show": {
          "resource": [
            "distTag"
          ],
          "operation": [
            "update"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "resource": "package",
    "operation": "getMetadata",
    "packageName": "",
    "packageVersion": "latest",
    "query": "",
    "limit": 10,
    "offset": 0,
    "distTagName": "latest"
  },
  "hasFilter": false,
  "fixedCollections": []
}