{
  "nodeType": "n8n-nodes-base.calendlyTrigger",
  "displayName": "Calendly Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "scope",
    "events"
  ],
  "optionalParameters": [
    "authentication",
    "deprecationNotice"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "apiKey",
        "enumValues": [
          "oAuth2",
          "apiKey"
        ]
      }
    },
    {
      "field": "deprecationNotice",
      "type": "notice",
      "required": false,
      "constraints": {
        "type": "notice",
        "default": ""
      },
      "displayOptions": {
        "show": {
          "authentication": [
            "apiKey"
          ]
        }
      },
      "dependsOn": [
        "authentication"
      ]
    },
    {
      "field": "scope",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": "user",
        "enumValues": [
          "organization",
          "user"
        ]
      }
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "constraints": {
        "type": "multiOptions",
        "default": [],
        "enumValues": [
          "invitee.created",
          "invitee.canceled"
        ]
      }
    }
  ],
  "computedDefaults": {
    "authentication": "apiKey",
    "deprecationNotice": "",
    "scope": "user",
    "events": []
  },
  "conditionalFields": [
    {
      "field": "deprecationNotice",
      "condition": {
        "show": {
          "authentication": [
            "apiKey"
          ]
        }
      },
      "dependsOnFields": [
        "authentication"
      ]
    }
  ],
  "fixedCollections": []
}