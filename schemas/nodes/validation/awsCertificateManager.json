{
  "nodeType": "n8n-nodes-base.awsCertificateManager",
  "displayName": "AWS Certificate Manager",
  "version": 1,
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "iam",
      "enumValues": [
        "iam",
        "assumeRole"
      ]
    },
    {
      "field": "resource",
      "type": "options",
      "required": false,
      "default": "certificate",
      "enumValues": [
        "certificate"
      ]
    },
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "renew",
      "enumValues": [
        "delete",
        "get",
        "getMany",
        "getMetadata",
        "renew"
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "certificate"
          ]
        }
      }
    },
    {
      "field": "certificateArn",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "resource": [
            "certificate"
          ],
          "operation": [
            "renew",
            "get",
            "delete",
            "getMetadata"
          ]
        }
      }
    },
    {
      "field": "bucketName",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "resource": [
            "certificate"
          ],
          "operation": [
            "delete"
          ]
        }
      }
    },
    {
      "field": "certificateKey",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "resource": [
            "certificate"
          ],
          "operation": [
            "delete"
          ]
        }
      }
    },
    {
      "field": "returnAll",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "operation": [
            "getMany"
          ],
          "resource": [
            "certificate"
          ]
        }
      }
    },
    {
      "field": "limit",
      "type": "number",
      "required": false,
      "default": 100,
      "displayOptions": {
        "show": {
          "operation": [
            "getMany"
          ],
          "resource": [
            "certificate"
          ],
          "returnAll": [
            false
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "resource": [
            "certificate"
          ],
          "operation": [
            "getMany"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "authentication": "iam",
    "resource": "certificate",
    "operation": "renew",
    "certificateArn": "",
    "bucketName": "",
    "certificateKey": "",
    "returnAll": false,
    "limit": 100,
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}