{
  "nodeType": "n8n-nodes-base.mauticTrigger",
  "displayName": "Mautic Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "events"
  ],
  "optionalParameters": [
    "authentication",
    "eventsOrder"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "credentials",
        "enumValues": [
          "credentials",
          "oAuth2"
        ]
      }
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "constraints": {
        "type": "multiOptions",
        "default": []
      }
    },
    {
      "field": "eventsOrder",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "ASC",
        "enumValues": [
          "ASC",
          "DESC"
        ]
      }
    }
  ],
  "computedDefaults": {
    "authentication": "credentials",
    "events": [],
    "eventsOrder": "ASC"
  },
  "conditionalFields": [],
  "fixedCollections": []
}