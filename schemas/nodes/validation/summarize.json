{
  "nodeType": "n8n-nodes-base.summarize",
  "displayName": "Summarize",
  "version": [
    1,
    1.1
  ],
  "rules": [
    {
      "field": "fieldsToSummarize",
      "type": "fixedCollection",
      "required": false,
      "default": {
        "values": [
          {
            "aggregation": "count",
            "field": ""
          }
        ]
      }
    },
    {
      "field": "fieldsToSplitBy",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "hide": {
          "/options.outputFormat": [
            "singleItem"
          ]
        }
      }
    },
    {
      "field": "fieldsToSplitBy",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "/options.outputFormat": [
            "singleItem"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "fieldsToSummarize": {
      "values": [
        {
          "aggregation": "count",
          "field": "",
          "includeEmpty": false,
          "separateBy": ",",
          "customSeparator": ""
        }
      ]
    },
    "fieldsToSplitBy": "",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": [
    "fieldsToSummarize"
  ]
}