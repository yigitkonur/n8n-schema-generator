{
  "nodeType": "n8n-nodes-base.surveyMonkeyTrigger",
  "displayName": "SurveyMonkey Trigger",
  "version": 1,
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "accessToken",
      "enumValues": [
        "accessToken",
        "oAuth2"
      ]
    },
    {
      "field": "objectType",
      "type": "options",
      "required": true,
      "default": "",
      "enumValues": [
        "collector",
        "survey"
      ]
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "default": "",
      "enumValues": [
        "collector_created",
        "collector_deleted",
        "collector_updated",
        "response_completed",
        "response_created",
        "response_deleted",
        "response_disqualified",
        "response_overquota",
        "response_updated",
        "survey_created",
        "survey_deleted",
        "survey_updated"
      ],
      "displayOptions": {
        "show": {
          "objectType": [
            "survey"
          ]
        }
      }
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "default": "",
      "enumValues": [
        "collector_deleted",
        "collector_updated",
        "response_completed",
        "response_created",
        "response_deleted",
        "response_disqualified",
        "response_overquota",
        "response_updated"
      ],
      "displayOptions": {
        "show": {
          "objectType": [
            "collector"
          ]
        }
      }
    },
    {
      "field": "surveyIds",
      "type": "multiOptions",
      "required": true,
      "default": [],
      "displayOptions": {
        "show": {
          "objectType": [
            "survey"
          ]
        },
        "hide": {
          "event": [
            "survey_created"
          ]
        }
      }
    },
    {
      "field": "surveyId",
      "type": "options",
      "required": true,
      "default": [],
      "displayOptions": {
        "show": {
          "objectType": [
            "collector"
          ]
        }
      }
    },
    {
      "field": "collectorIds",
      "type": "multiOptions",
      "required": true,
      "default": [],
      "displayOptions": {
        "show": {
          "objectType": [
            "collector"
          ]
        }
      }
    },
    {
      "field": "resolveData",
      "type": "boolean",
      "required": false,
      "default": true,
      "displayOptions": {
        "show": {
          "event": [
            "response_completed"
          ]
        }
      }
    },
    {
      "field": "onlyAnswers",
      "type": "boolean",
      "required": false,
      "default": true,
      "displayOptions": {
        "show": {
          "resolveData": [
            true
          ],
          "event": [
            "response_completed"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "authentication": "accessToken",
    "objectType": "",
    "surveyIds": [],
    "surveyId": [],
    "collectorIds": [],
    "resolveData": true,
    "onlyAnswers": true
  },
  "hasFilter": false,
  "fixedCollections": []
}