{
  "nodeType": "n8n-nodes-base.googleSheetsTrigger",
  "displayName": "Google Sheets Trigger",
  "version": 1,
  "rules": [
    {
      "field": "authentication",
      "type": "hidden",
      "required": false,
      "default": "triggerOAuth2"
    },
    {
      "field": "documentId",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "list",
        "value": ""
      }
    },
    {
      "field": "sheetName",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "list",
        "value": ""
      }
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "default": "anyUpdate",
      "enumValues": [
        "rowAdded",
        "rowUpdate",
        "anyUpdate"
      ]
    },
    {
      "field": "includeInOutput",
      "type": "options",
      "required": false,
      "default": "new",
      "enumValues": [
        "new",
        "old",
        "both"
      ],
      "displayOptions": {
        "hide": {
          "event": [
            "rowAdded"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "authentication": "triggerOAuth2",
    "documentId": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "sheetName": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "event": "anyUpdate",
    "includeInOutput": "new",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}