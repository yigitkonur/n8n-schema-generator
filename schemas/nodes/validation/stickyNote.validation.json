{
  "nodeType": "n8n-nodes-base.stickyNote",
  "displayName": "Sticky Note",
  "version": 1,
  "category": [
    "input"
  ],
  "requiredParameters": [
    "height",
    "width",
    "color"
  ],
  "optionalParameters": [
    "content"
  ],
  "validationRules": [
    {
      "field": "content",
      "type": "string",
      "required": false,
      "constraints": {
        "type": "string",
        "default": "## I'm a note \n**Double click** to edit me. [Guide](https://docs.n8n.io/workflows/components/sticky-notes/)"
      }
    },
    {
      "field": "height",
      "type": "number",
      "required": true,
      "constraints": {
        "type": "number",
        "default": 160
      }
    },
    {
      "field": "width",
      "type": "number",
      "required": true,
      "constraints": {
        "type": "number",
        "default": 240
      }
    },
    {
      "field": "color",
      "type": "number",
      "required": true,
      "constraints": {
        "type": "number",
        "default": 1
      }
    }
  ],
  "computedDefaults": {
    "content": "## I'm a note \n**Double click** to edit me. [Guide](https://docs.n8n.io/workflows/components/sticky-notes/)",
    "height": 160,
    "width": 240,
    "color": 1
  },
  "conditionalFields": [],
  "fixedCollections": []
}