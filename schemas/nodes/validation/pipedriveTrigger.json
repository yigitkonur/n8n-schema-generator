{
  "nodeType": "n8n-nodes-base.pipedriveTrigger",
  "displayName": "Pipedrive Trigger",
  "version": [
    1,
    1.1
  ],
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "apiToken",
      "enumValues": [
        "apiToken",
        "oAuth2"
      ]
    },
    {
      "field": "incomingAuthentication",
      "type": "options",
      "required": false,
      "default": "none",
      "enumValues": [
        "basicAuth",
        "none"
      ]
    },
    {
      "field": "action",
      "type": "options",
      "required": false,
      "default": "*",
      "enumValues": [
        "added",
        "*",
        "deleted",
        "merged",
        "updated"
      ],
      "displayOptions": {
        "hide": {
          "@version": [
            {
              "_cnd": {
                "gte": 1.1
              }
            }
          ]
        }
      }
    },
    {
      "field": "action",
      "type": "options",
      "required": false,
      "default": "*",
      "enumValues": [
        "*",
        "create",
        "delete",
        "change"
      ],
      "displayOptions": {
        "hide": {
          "@version": [
            {
              "_cnd": {
                "lte": 1
              }
            }
          ]
        }
      }
    },
    {
      "field": "entity",
      "type": "options",
      "required": false,
      "default": "*",
      "enumValues": [
        "activity",
        "activityType",
        "*",
        "deal",
        "note",
        "organization",
        "person",
        "pipeline",
        "product",
        "stage",
        "user"
      ],
      "displayOptions": {
        "hide": {
          "@version": [
            {
              "_cnd": {
                "lte": 1
              }
            }
          ]
        }
      }
    },
    {
      "field": "object",
      "type": "options",
      "required": false,
      "default": "*",
      "enumValues": [
        "activity",
        "activityType",
        "*",
        "deal",
        "note",
        "organization",
        "person",
        "pipeline",
        "product",
        "stage",
        "user"
      ],
      "displayOptions": {
        "hide": {
          "@version": [
            {
              "_cnd": {
                "gte": 1.1
              }
            }
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "authentication": "apiToken",
    "incomingAuthentication": "none",
    "action": "*",
    "entity": "*",
    "object": "*"
  },
  "hasFilter": false,
  "fixedCollections": []
}