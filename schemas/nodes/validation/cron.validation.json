                "type": "string",
                "default": "* * * * * *"
              },
              "value": {
                "type": "number",
                "default": 2,
                "minValue": 0,
                "maxValue": 1000
              },
              "unit": {
                "type": "options",
                "default": "hours",
                "enumValues": [
                  "minutes",
                  "hours"
                ]
