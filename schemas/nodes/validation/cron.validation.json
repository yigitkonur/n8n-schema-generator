                "default": "everyDay",
                "enumValues": [
                  "everyMinute",
                  "everyHour",
                  "everyDay",
                  "everyWeek",
                  "everyMonth",
                  "everyX",
                  "custom"
                ]
              },
              "hour": {
                "type": "number",
                "default": 14,
                "minValue": 0,
                "maxValue": 23
              },
              "minute": {
                "type": "number",
                "default": 0,
                "minValue": 0,
                "maxValue": 59
              },
              "dayOfMonth": {
