{
  "nodeType": "n8n-nodes-base.seaTableTrigger",
  "displayName": "SeaTable Trigger",
  "version": 2,
  "defaultVersion": 2,
  "rules": [
    {
      "field": "event",
      "type": "options",
      "required": false,
      "default": "newRow",
      "enumValues": [
        "newRow",
        "updatedRow",
        "newAsset"
      ]
    },
    {
      "field": "tableName",
      "type": "options",
      "required": true,
      "default": ""
    },
    {
      "field": "viewName",
      "type": "options",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "event": [
            "newRow",
            "updatedRow"
          ]
        }
      }
    },
    {
      "field": "assetColumn",
      "type": "options",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "event": [
            "newAsset"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    },
    {
      "field": "notice",
      "type": "notice",
      "required": false,
      "default": ""
    }
  ],
  "computedDefaults": {
    "event": "newRow",
    "tableName": "",
    "viewName": "",
    "assetColumn": "",
    "options": {},
    "notice": ""
  },
  "hasFilter": false,
  "fixedCollections": []
}