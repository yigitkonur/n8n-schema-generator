{
  "nodeType": "n8n-nodes-base.emailReadImap",
  "displayName": "Email Trigger (IMAP)",
  "version": [
    2,
    2.1
  ],
  "rules": [
    {
      "field": "mailbox",
      "type": "string",
      "required": false,
      "default": "INBOX"
    },
    {
      "field": "postProcessAction",
      "type": "options",
      "required": false,
      "default": "read",
      "enumValues": [
        "read",
        "nothing"
      ]
    },
    {
      "field": "downloadAttachments",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "format": [
            "simple"
          ]
        }
      }
    },
    {
      "field": "format",
      "type": "options",
      "required": false,
      "default": "simple",
      "enumValues": [
        "raw",
        "resolved",
        "simple"
      ]
    },
    {
      "field": "dataPropertyAttachmentsPrefixName",
      "type": "string",
      "required": false,
      "default": "attachment_",
      "displayOptions": {
        "show": {
          "format": [
            "resolved"
          ]
        }
      }
    },
    {
      "field": "dataPropertyAttachmentsPrefixName",
      "type": "string",
      "required": false,
      "default": "attachment_",
      "displayOptions": {
        "show": {
          "format": [
            "simple"
          ],
          "downloadAttachments": [
            true
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "mailbox": "INBOX",
    "postProcessAction": "read",
    "downloadAttachments": false,
    "format": "simple",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}