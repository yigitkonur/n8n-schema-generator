{
  "nodeType": "n8n-nodes-base.airtableTrigger",
  "displayName": "Airtable Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "baseId",
    "tableId",
    "triggerField"
  ],
  "optionalParameters": [
    "authentication",
    "downloadAttachments",
    "downloadFieldNames",
    "additionalFields"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "airtableApi",
        "enumValues": [
          "airtableApi",
          "airtableTokenApi",
          "airtableOAuth2Api"
        ]
      }
    },
    {
      "field": "baseId",
      "type": "resourceLocator",
      "required": true,
      "constraints": {
        "type": "resourceLocator",
        "default": {
          "mode": "url",
          "value": ""
        }
      }
    },
    {
      "field": "tableId",
      "type": "resourceLocator",
      "required": true,
      "constraints": {
        "type": "resourceLocator",
        "default": {
          "mode": "url",
          "value": ""
        }
      }
    },
    {
      "field": "triggerField",
      "type": "string",
      "required": true,
      "constraints": {
        "type": "string",
        "default": ""
      }
    },
    {
      "field": "downloadAttachments",
      "type": "boolean",
      "required": false,
      "constraints": {
        "type": "boolean",
        "default": false
      }
    },
    {
      "field": "downloadFieldNames",
      "type": "string",
      "required": true,
      "constraints": {
        "type": "string",
        "default": ""
      },
      "displayOptions": {
        "show": {
          "downloadAttachments": [
            true
          ]
        }
      },
      "dependsOn": [
        "downloadAttachments"
      ]
    },
    {
      "field": "additionalFields",
      "type": "collection",
      "required": false,
      "constraints": {
        "type": "collection",
        "default": {},
        "properties": {},
        "requiredProperties": []
      }
    }
  ],
  "computedDefaults": {
    "authentication": "airtableApi",
    "baseId": {
      "__rl": true,
      "mode": "url",
      "value": ""
    },
    "tableId": {
      "__rl": true,
      "mode": "url",
      "value": ""
    },
    "triggerField": "",
    "downloadAttachments": false,
    "downloadFieldNames": "",
    "additionalFields": {}
  },
  "conditionalFields": [
    {
      "field": "downloadFieldNames",
      "condition": {
        "show": {
          "downloadAttachments": [
            true
          ]
        }
      },
      "dependsOnFields": [
        "downloadAttachments"
      ]
    }
  ],
  "fixedCollections": []
}