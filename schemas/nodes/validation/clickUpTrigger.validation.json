{
  "nodeType": "n8n-nodes-base.clickUpTrigger",
  "displayName": "ClickUp Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "team",
    "events"
  ],
  "optionalParameters": [
    "authentication",
    "filters"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "accessToken",
        "enumValues": [
          "accessToken",
          "oAuth2"
        ]
      }
    },
    {
      "field": "team",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": ""
      }
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "constraints": {
        "type": "multiOptions",
        "default": [],
        "enumValues": [
          "*",
          "folderCreated",
          "folderDeleted",
          "folderUpdated",
          "goalCreated",
          "goalDeleted",
          "goalUpdated",
          "keyResultCreated",
          "keyResultDelete",
          "keyResultUpdated",
          "listCreated",
          "listDeleted",
          "listUpdated",
          "spaceCreated",
          "spaceDeleted",
          "spaceUpdated",
          "taskAssigneeUpdated",
          "taskCommentPosted",
          "taskCommentUpdated",
          "taskCreated",
          "taskDeleted",
          "taskDueDateUpdated",
          "taskMoved",
          "taskStatusUpdated",
          "taskTagUpdated",
          "taskTimeEstimateUpdated",
          "taskTimeTrackedUpdated",
          "taskUpdated"
        ]
      }
    },
    {
      "field": "filters",
      "type": "collection",
      "required": false,
      "constraints": {
        "type": "collection",
        "default": {},
        "properties": {},
        "requiredProperties": []
      }
    }
  ],
  "computedDefaults": {
    "authentication": "accessToken",
    "team": "",
    "events": [],
    "filters": {}
  },
  "conditionalFields": [],
  "fixedCollections": []
}