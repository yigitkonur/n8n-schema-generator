{
  "nodeType": "n8n-nodes-base.asanaTrigger",
  "displayName": "Asana Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "resource"
  ],
  "optionalParameters": [
    "authentication",
    "workspace"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "accessToken",
        "enumValues": [
          "accessToken",
          "oAuth2"
        ]
      }
    },
    {
      "field": "resource",
      "type": "string",
      "required": true,
      "constraints": {
        "type": "string",
        "default": ""
      }
    },
    {
      "field": "workspace",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "",
        "enumValues": []
      }
    }
  ],
  "computedDefaults": {
    "authentication": "accessToken",
    "resource": "",
    "workspace": ""
  },
  "conditionalFields": [],
  "fixedCollections": []
}