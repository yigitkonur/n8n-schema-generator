{
  "nodeType": "n8n-nodes-base.taigaTrigger",
  "displayName": "Taiga Trigger",
  "version": 1,
  "rules": [
    {
      "field": "projectId",
      "type": "options",
      "required": true,
      "default": ""
    },
    {
      "field": "resources",
      "type": "multiOptions",
      "required": true,
      "default": [
        "all"
      ],
      "enumValues": [
        "all",
        "issue",
        "milestone",
        "task",
        "userstory",
        "wikipage"
      ]
    },
    {
      "field": "operations",
      "type": "multiOptions",
      "required": true,
      "default": [
        "all"
      ],
      "enumValues": [
        "all",
        "create",
        "delete",
        "change"
      ]
    }
  ],
  "computedDefaults": {
    "projectId": "",
    "resources": [
      "all"
    ],
    "operations": [
      "all"
    ]
  },
  "hasFilter": false,
  "fixedCollections": []
}