{
  "nodeType": "n8n-nodes-base.theHiveProjectTrigger",
  "displayName": "TheHive 5 Trigger",
  "version": 1,
  "rules": [
    {
      "field": "notice",
      "type": "notice",
      "required": false,
      "default": ""
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "default": [],
      "enumValues": [
        "*",
        "alert_create",
        "alert_delete",
        "alert_update",
        "case_create",
        "case_delete",
        "case_update",
        "comment_create",
        "comment_delete",
        "comment_update",
        "observable_create",
        "observable_delete",
        "observable_update",
        "page_create",
        "page_delete",
        "page_update",
        "task_create",
        "task_update",
        "log_create",
        "log_delete",
        "log_update"
      ]
    },
    {
      "field": "filters",
      "type": "fixedCollection",
      "required": false,
      "default": {}
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "notice": "",
    "events": [],
    "filters": {},
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": [
    "filters"
  ]
}