{
  "nodeType": "n8n-nodes-base.webflowTrigger",
  "displayName": "Webflow Trigger",
  "version": 2,
  "defaultVersion": 2,
  "rules": [
    {
      "field": "site",
      "type": "options",
      "required": true,
      "default": ""
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "default": "form_submission",
      "enumValues": [
        "collection_item_created",
        "collection_item_deleted",
        "collection_item_changed",
        "ecomm_inventory_changed",
        "ecomm_new_order",
        "ecomm_order_changed",
        "form_submission",
        "site_publish"
      ]
    }
  ],
  "computedDefaults": {
    "site": "",
    "event": "form_submission"
  },
  "hasFilter": false,
  "fixedCollections": []
}