{
  "nodeType": "n8n-nodes-base.oura",
  "displayName": "Oura",
  "version": 1,
  "rules": [
    {
      "field": "resource",
      "type": "options",
      "required": false,
      "default": "summary",
      "enumValues": [
        "profile",
        "summary"
      ]
    },
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "get",
      "enumValues": [
        "get"
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "profile"
          ]
        }
      }
    },
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "getSleep",
      "enumValues": [
        "getActivity",
        "getReadiness",
        "getSleep"
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "summary"
          ]
        }
      }
    },
    {
      "field": "returnAll",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "resource": [
            "summary"
          ]
        }
      }
    },
    {
      "field": "limit",
      "type": "number",
      "required": false,
      "default": 5,
      "displayOptions": {
        "show": {
          "resource": [
            "summary"
          ],
          "returnAll": [
            false
          ]
        }
      }
    },
    {
      "field": "filters",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "resource": [
            "summary"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "resource": "summary",
    "operation": "getSleep",
    "returnAll": false,
    "limit": 5,
    "filters": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}