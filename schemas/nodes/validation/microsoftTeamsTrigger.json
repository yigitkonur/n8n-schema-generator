{
  "nodeType": "n8n-nodes-base.microsoftTeamsTrigger",
  "displayName": "Microsoft Teams Trigger",
  "version": 1,
  "rules": [
    {
      "field": "event",
      "type": "options",
      "required": false,
      "default": "newChannelMessage",
      "enumValues": [
        "newChannel",
        "newChannelMessage",
        "newChat",
        "newChatMessage",
        "newTeamMember"
      ]
    },
    {
      "field": "watchAllTeams",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "event": [
            "newChannel",
            "newChannelMessage",
            "newTeamMember"
          ]
        }
      }
    },
    {
      "field": "teamId",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "list",
        "value": ""
      },
      "displayOptions": {
        "show": {
          "event": [
            "newChannel",
            "newChannelMessage",
            "newTeamMember"
          ],
          "watchAllTeams": [
            false
          ]
        }
      }
    },
    {
      "field": "watchAllChannels",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "event": [
            "newChannelMessage"
          ],
          "watchAllTeams": [
            false
          ]
        }
      }
    },
    {
      "field": "channelId",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "list",
        "value": ""
      },
      "displayOptions": {
        "show": {
          "event": [
            "newChannelMessage"
          ],
          "watchAllTeams": [
            false
          ],
          "watchAllChannels": [
            false
          ]
        }
      }
    },
    {
      "field": "watchAllChats",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "event": [
            "newChatMessage"
          ]
        }
      }
    },
    {
      "field": "chatId",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "list",
        "value": ""
      },
      "displayOptions": {
        "show": {
          "event": [
            "newChatMessage"
          ],
          "watchAllChats": [
            false
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "event": "newChannelMessage",
    "watchAllTeams": false,
    "teamId": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "watchAllChannels": false,
    "channelId": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "watchAllChats": false,
    "chatId": {
      "__rl": true,
      "mode": "list",
      "value": ""
    }
  },
  "hasFilter": false,
  "fixedCollections": []
}