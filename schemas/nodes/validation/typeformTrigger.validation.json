{
  "nodeType": "n8n-nodes-base.typeformTrigger",
  "displayName": "Typeform Trigger",
  "version": [
    1,
    1.1
  ],
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "formId"
  ],
  "optionalParameters": [
    "authentication",
    "simplifyAnswers",
    "onlyAnswers"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "accessToken",
        "enumValues": [
          "accessToken",
          "oAuth2"
        ]
      }
    },
    {
      "field": "formId",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": "",
        "enumValues": []
      }
    },
    {
      "field": "simplifyAnswers",
      "type": "boolean",
      "required": false,
      "constraints": {
        "type": "boolean",
        "default": true
      }
    },
    {
      "field": "onlyAnswers",
      "type": "boolean",
      "required": false,
      "constraints": {
        "type": "boolean",
        "default": true
      }
    }
  ],
  "computedDefaults": {
    "authentication": "accessToken",
    "formId": "",
    "simplifyAnswers": true,
    "onlyAnswers": true
  },
  "conditionalFields": [],
  "fixedCollections": []
}