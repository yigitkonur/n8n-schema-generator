{
  "nodeType": "n8n-nodes-base.acuitySchedulingTrigger",
  "displayName": "Acuity Scheduling Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "event"
  ],
  "optionalParameters": [
    "authentication",
    "resolveData"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "apiKey",
        "enumValues": [
          "apiKey",
          "oAuth2"
        ]
      }
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": "",
        "enumValues": [
          "appointment.canceled",
          "appointment.changed",
          "appointment.rescheduled",
          "appointment.scheduled",
          "order.completed"
        ]
      }
    },
    {
      "field": "resolveData",
      "type": "boolean",
      "required": false,
      "constraints": {
        "type": "boolean",
        "default": true
      }
    }
  ],
  "computedDefaults": {
    "authentication": "apiKey",
    "event": "",
    "resolveData": true
  },
  "conditionalFields": [],
  "fixedCollections": []
}