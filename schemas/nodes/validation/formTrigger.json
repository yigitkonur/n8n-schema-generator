{
  "nodeType": "n8n-nodes-base.formTrigger",
  "displayName": "n8n Form Trigger",
  "version": [
    2,
    2.1,
    2.2,
    2.3
  ],
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "none",
      "enumValues": [
        "basicAuth",
        "none"
      ]
    },
    {
      "field": "path",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "@version": [
            {
              "_cnd": {
                "lte": 2.1
              }
            }
          ]
        }
      }
    },
    {
      "field": "formTitle",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "formDescription",
      "type": "string",
      "required": false,
      "default": ""
    },
    {
      "field": "formFields",
      "type": "fixedCollection",
      "required": false,
      "default": {}
    },
    {
      "field": "responseMode",
      "type": "options",
      "required": false,
      "default": "onReceived",
      "enumValues": [
        "onReceived",
        "lastNode",
        "responseNode"
      ],
      "displayOptions": {
        "show": {
          "@version": [
            {
              "_cnd": {
                "lte": 2.1
              }
            }
          ]
        }
      }
    },
    {
      "field": "responseMode",
      "type": "options",
      "required": false,
      "default": "onReceived",
      "enumValues": [
        "onReceived",
        "lastNode"
      ],
      "displayOptions": {
        "show": {
          "@version": [
            {
              "_cnd": {
                "gte": 2.2
              }
            }
          ]
        }
      }
    },
    {
      "field": "formNotice",
      "type": "notice",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "responseMode": [
            "responseNode"
          ]
        }
      }
    },
    {
      "field": "addFormPage",
      "type": "notice",
      "required": false,
      "default": ""
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "authentication": "none",
    "path": "",
    "formTitle": "",
    "formDescription": "",
    "formFields": {},
    "responseMode": "onReceived",
    "formNotice": "",
    "addFormPage": "",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": [
    "formFields"
  ]
}