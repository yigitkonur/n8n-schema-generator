{
  "nodeType": "n8n-nodes-base.linearTrigger",
  "displayName": "Linear Trigger",
  "version": 1,
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "apiToken",
      "enumValues": [
        "apiToken",
        "oAuth2"
      ]
    },
    {
      "field": "notice",
      "type": "notice",
      "required": false,
      "default": ""
    },
    {
      "field": "teamId",
      "type": "options",
      "required": false,
      "default": ""
    },
    {
      "field": "resources",
      "type": "multiOptions",
      "required": true,
      "default": [],
      "enumValues": [
        "reaction",
        "cycle",
        "issue",
        "comment",
        "issueLabel",
        "project"
      ]
    }
  ],
  "computedDefaults": {
    "authentication": "apiToken",
    "notice": "",
    "teamId": "",
    "resources": []
  },
  "hasFilter": false,
  "fixedCollections": []
}