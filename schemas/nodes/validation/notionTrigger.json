{
  "nodeType": "n8n-nodes-base.notionTrigger",
  "displayName": "Notion Trigger",
  "version": 1,
  "rules": [
    {
      "field": "event",
      "type": "options",
      "required": true,
      "default": "pageAddedToDatabase",
      "enumValues": [
        "pageAddedToDatabase",
        "pagedUpdatedInDatabase"
      ]
    },
    {
      "field": "notionNotice",
      "type": "notice",
      "required": false,
      "default": ""
    },
    {
      "field": "databaseId",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "list",
        "value": ""
      },
      "displayOptions": {
        "show": {
          "event": [
            "pageAddedToDatabase",
            "pagedUpdatedInDatabase"
          ]
        }
      }
    },
    {
      "field": "simple",
      "type": "boolean",
      "required": false,
      "default": true,
      "displayOptions": {
        "show": {
          "event": [
            "pageAddedToDatabase",
            "pagedUpdatedInDatabase"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "event": "pageAddedToDatabase",
    "notionNotice": "",
    "databaseId": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "simple": true
  },
  "hasFilter": false,
  "fixedCollections": []
}