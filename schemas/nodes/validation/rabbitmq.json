{
  "nodeType": "n8n-nodes-base.rabbitmq",
  "displayName": "RabbitMQ",
  "version": [
    1,
    1.1
  ],
  "rules": [
    {
      "field": "operation",
      "type": "hidden",
      "required": false,
      "default": "sendMessage",
      "displayOptions": {
        "show": {
          "@version": [
            1
          ]
        }
      }
    },
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "sendMessage",
      "enumValues": [
        "sendMessage",
        "deleteMessage"
      ],
      "displayOptions": {
        "show": {
          "@version": [
            1.1
          ]
        }
      }
    },
    {
      "field": "deleteMessage",
      "type": "notice",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "deleteMessage"
          ]
        }
      }
    },
    {
      "field": "mode",
      "type": "options",
      "required": false,
      "default": "queue",
      "enumValues": [
        "queue",
        "exchange"
      ],
      "displayOptions": {
        "hide": {
          "operation": [
            "deleteMessage"
          ]
        }
      }
    },
    {
      "field": "queue",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "mode": [
            "queue"
          ]
        },
        "hide": {
          "operation": [
            "deleteMessage"
          ]
        }
      }
    },
    {
      "field": "exchange",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "mode": [
            "exchange"
          ]
        }
      }
    },
    {
      "field": "exchangeType",
      "type": "options",
      "required": false,
      "default": "fanout",
      "enumValues": [
        "direct",
        "topic",
        "headers",
        "fanout"
      ],
      "displayOptions": {
        "show": {
          "mode": [
            "exchange"
          ]
        }
      }
    },
    {
      "field": "routingKey",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "mode": [
            "exchange"
          ]
        }
      }
    },
    {
      "field": "sendInputData",
      "type": "boolean",
      "required": false,
      "default": true,
      "displayOptions": {
        "show": {
          "operation": [
            "sendMessage"
          ]
        }
      }
    },
    {
      "field": "message",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "sendInputData": [
            false
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "operation": [
            "sendMessage"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "operation": "sendMessage",
    "deleteMessage": "",
    "mode": "queue",
    "queue": "",
    "exchange": "",
    "exchangeType": "fanout",
    "routingKey": "",
    "sendInputData": true,
    "message": "",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}