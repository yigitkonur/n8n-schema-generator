{
  "nodeType": "n8n-nodes-base.slackTrigger",
  "displayName": "Slack Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [],
  "optionalParameters": [
    "authentication",
    "notice",
    "trigger",
    "watchWorkspace",
    "notice",
    "channelId",
    "downloadFiles",
    "options"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "hidden",
      "required": false,
      "constraints": {
        "type": "hidden",
        "default": "accessToken"
      }
    },
    {
      "field": "notice",
      "type": "notice",
      "required": false,
      "constraints": {
        "type": "notice",
        "default": ""
      }
    },
    {
      "field": "trigger",
      "type": "multiOptions",
      "required": false,
      "constraints": {
        "type": "multiOptions",
        "default": [],
        "enumValues": [
          "any_event",
          "app_mention",
          "file_public",
          "file_share",
          "message",
          "channel_created",
          "team_join",
          "reaction_added"
        ]
      }
    },
    {
      "field": "watchWorkspace",
      "type": "boolean",
      "required": false,
      "constraints": {
        "type": "boolean",
        "default": false
      },
      "displayOptions": {
        "show": {
          "trigger": [
            "any_event",
            "message",
            "reaction_added",
            "file_share",
            "app_mention"
          ]
        }
      },
      "dependsOn": [
        "trigger"
      ]
    },
    {
      "field": "notice",
      "type": "notice",
      "required": false,
      "constraints": {
        "type": "notice",
        "default": ""
      },
      "displayOptions": {
        "show": {
          "trigger": [
            "any_event",
            "message",
            "reaction_added",
            "file_share",
            "app_mention"
          ],
          "watchWorkspace": [
            true
          ]
        }
      },
      "dependsOn": [
        "trigger",
        "watchWorkspace"
      ]
    },
    {
      "field": "channelId",
      "type": "resourceLocator",
      "required": true,
      "constraints": {
        "type": "resourceLocator",
        "default": {
          "mode": "list",
          "value": ""
        }
      },
      "displayOptions": {
        "show": {
          "watchWorkspace": [
            false
          ]
        }
      },
      "dependsOn": [
        "watchWorkspace"
      ]
    },
    {
      "field": "downloadFiles",
      "type": "boolean",
      "required": false,
      "constraints": {
        "type": "boolean",
        "default": false
      },
      "displayOptions": {
        "show": {
          "trigger": [
            "any_event",
            "file_share"
          ]
        }
      },
      "dependsOn": [
        "trigger"
      ]
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "constraints": {
        "type": "collection",
        "default": {},
        "properties": {},
        "requiredProperties": []
      }
    }
  ],
  "computedDefaults": {
    "authentication": "accessToken",
    "notice": "",
    "trigger": [],
    "watchWorkspace": false,
    "channelId": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "downloadFiles": false,
    "options": {}
  },
  "conditionalFields": [
    {
      "field": "watchWorkspace",
      "condition": {
        "show": {
          "trigger": [
            "any_event",
            "message",
            "reaction_added",
            "file_share",
            "app_mention"
          ]
        }
      },
      "dependsOnFields": [
        "trigger"
      ]
    },
    {
      "field": "notice",
      "condition": {
        "show": {
          "trigger": [
            "any_event",
            "message",
            "reaction_added",
            "file_share",
            "app_mention"
          ],
          "watchWorkspace": [
            true
          ]
        }
      },
      "dependsOnFields": [
        "trigger",
        "watchWorkspace"
      ]
    },
    {
      "field": "channelId",
      "condition": {
        "show": {
          "watchWorkspace": [
            false
          ]
        }
      },
      "dependsOnFields": [
        "watchWorkspace"
      ]
    },
    {
      "field": "downloadFiles",
      "condition": {
        "show": {
          "trigger": [
            "any_event",
            "file_share"
          ]
        }
      },
      "dependsOnFields": [
        "trigger"
      ]
    }
  ],
  "fixedCollections": []
}