{
  "nodeType": "n8n-nodes-base.mailchimpTrigger",
  "displayName": "Mailchimp Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "list",
    "events",
    "sources"
  ],
  "optionalParameters": [
    "authentication"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "apiKey",
        "enumValues": [
          "apiKey",
          "oAuth2"
        ]
      }
    },
    {
      "field": "list",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": "",
        "enumValues": []
      }
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "constraints": {
        "type": "multiOptions",
        "default": [],
        "enumValues": [
          "campaign",
          "cleaned",
          "upemail",
          "profile",
          "subscribe",
          "unsubscribe"
        ]
      }
    },
    {
      "field": "sources",
      "type": "multiOptions",
      "required": true,
      "constraints": {
        "type": "multiOptions",
        "default": [],
        "enumValues": [
          "user",
          "admin",
          "api"
        ]
      }
    }
  ],
  "computedDefaults": {
    "authentication": "apiKey",
    "list": "",
    "events": [],
    "sources": []
  },
  "conditionalFields": [],
  "fixedCollections": []
}