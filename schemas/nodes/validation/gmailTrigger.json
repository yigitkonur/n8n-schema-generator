{
  "nodeType": "n8n-nodes-base.gmailTrigger",
  "displayName": "Gmail Trigger",
  "version": [
    1,
    1.1,
    1.2,
    1.3
  ],
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "oAuth2",
      "enumValues": [
        "oAuth2",
        "serviceAccount"
      ]
    },
    {
      "field": "event",
      "type": "options",
      "required": false,
      "default": "messageReceived",
      "enumValues": [
        "messageReceived"
      ]
    },
    {
      "field": "simple",
      "type": "boolean",
      "required": false,
      "default": true
    },
    {
      "field": "filters",
      "type": "collection",
      "required": false,
      "default": {}
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "hide": {
          "simple": [
            true
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "authentication": "oAuth2",
    "event": "messageReceived",
    "simple": true,
    "filters": {},
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}