{
  "nodeType": "n8n-nodes-base.boxTrigger",
  "displayName": "Box Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "events"
  ],
  "optionalParameters": [
    "targetType",
    "targetId"
  ],
  "validationRules": [
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "constraints": {
        "type": "multiOptions",
        "default": [],
        "enumValues": [
          "COLLABORATION.ACCEPTED",
          "COLLABORATION.CREATED",
          "COLLABORATION.REJECTED",
          "COLLABORATION.REMOVED",
          "COLLABORATION.UPDATED",
          "COMMENT.CREATED",
          "COMMENT.DELETED",
          "COMMENT.UPDATED",
          "FILE.COPIED",
          "FILE.DELETED",
          "FILE.DOWNLOADED",
          "FILE.LOCKED",
          "FILE.MOVED",
          "FILE.PREVIEWED",
          "FILE.RENAMED",
          "FILE.RESTORED",
          "FILE.TRASHED",
          "FILE.UNLOCKED",
          "FILE.UPLOADED",
          "FOLDER.COPIED",
          "FOLDER.CREATED",
          "FOLDER.DELETED",
          "FOLDER.DOWNLOADED",
          "FOLDER.MOVED",
          "FOLDER.RENAMED",
          "FOLDER.RESTORED",
          "FOLDER.TRASHED",
          "METADATA_INSTANCE.CREATED",
          "METADATA_INSTANCE.DELETED",
          "METADATA_INSTANCE.UPDATED",
          "SHARED_LINK.CREATED",
          "SHARED_LINK.DELETED",
          "SHARED_LINK.UPDATED",
          "TASK_ASSIGNMENT.CREATED",
          "TASK_ASSIGNMENT.UPDATED",
          "WEBHOOK.DELETED"
        ]
      }
    },
    {
      "field": "targetType",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "",
        "enumValues": [
          "file",
          "folder"
        ]
      }
    },
    {
      "field": "targetId",
      "type": "string",
      "required": false,
      "constraints": {
        "type": "string",
        "default": ""
      }
    }
  ],
  "computedDefaults": {
    "events": [],
    "targetType": "",
    "targetId": ""
  },
  "conditionalFields": [],
  "fixedCollections": []
}