{
  "nodeType": "n8n-nodes-base.graphql",
  "displayName": "GraphQL",
  "version": [
    1,
    1.1
  ],
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "none",
      "enumValues": [
        "basicAuth",
        "customAuth",
        "digestAuth",
        "headerAuth",
        "none",
        "oAuth1",
        "oAuth2",
        "queryAuth"
      ]
    },
    {
      "field": "requestMethod",
      "type": "options",
      "required": false,
      "default": "POST",
      "enumValues": [
        "GET",
        "POST"
      ]
    },
    {
      "field": "endpoint",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "allowUnauthorizedCerts",
      "type": "boolean",
      "required": false,
      "default": false
    },
    {
      "field": "requestFormat",
      "type": "options",
      "required": true,
      "default": "graphql",
      "enumValues": [
        "graphql",
        "json"
      ],
      "displayOptions": {
        "show": {
          "requestMethod": [
            "POST"
          ],
          "@version": [
            1
          ]
        }
      }
    },
    {
      "field": "requestFormat",
      "type": "options",
      "required": true,
      "default": "json",
      "enumValues": [
        "json",
        "graphql"
      ],
      "displayOptions": {
        "show": {
          "requestMethod": [
            "POST"
          ],
          "@version": [
            {
              "_cnd": {
                "gte": 1.1
              }
            }
          ]
        }
      }
    },
    {
      "field": "query",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "variables",
      "type": "json",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "requestFormat": [
            "json"
          ],
          "requestMethod": [
            "POST"
          ]
        }
      }
    },
    {
      "field": "operationName",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "requestFormat": [
            "json"
          ],
          "requestMethod": [
            "POST"
          ]
        }
      }
    },
    {
      "field": "responseFormat",
      "type": "options",
      "required": false,
      "default": "json",
      "enumValues": [
        "json",
        "string"
      ]
    },
    {
      "field": "dataPropertyName",
      "type": "string",
      "required": true,
      "default": "data",
      "displayOptions": {
        "show": {
          "responseFormat": [
            "string"
          ]
        }
      }
    },
    {
      "field": "headerParametersUi",
      "type": "fixedCollection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "authentication": "none",
    "requestMethod": "POST",
    "endpoint": "",
    "allowUnauthorizedCerts": false,
    "requestFormat": "graphql",
    "query": "",
    "variables": "",
    "operationName": "",
    "responseFormat": "json",
    "dataPropertyName": "data",
    "headerParametersUi": {}
  },
  "hasFilter": false,
  "fixedCollections": [
    "headerParametersUi"
  ]
}