{
  "nodeType": "n8n-nodes-base.eventbriteTrigger",
  "displayName": "Eventbrite Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "organization",
    "event",
    "actions"
  ],
  "optionalParameters": [
    "authentication",
    "resolveData"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "privateKey",
        "enumValues": [
          "privateKey",
          "oAuth2"
        ]
      }
    },
    {
      "field": "organization",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": ""
      }
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": ""
      }
    },
    {
      "field": "actions",
      "type": "multiOptions",
      "required": true,
      "constraints": {
        "type": "multiOptions",
        "default": [],
        "enumValues": [
          "attendee.checked_in",
          "attendee.checked_out",
          "attendee.updated",
          "event.created",
          "event.published",
          "event.unpublished",
          "event.updated",
          "order.placed",
          "order.refunded",
          "order.updated",
          "organizer.updated",
          "ticket_class.created",
          "ticket_class.deleted",
          "ticket_class.updated",
          "venue.updated"
        ]
      }
    },
    {
      "field": "resolveData",
      "type": "boolean",
      "required": false,
      "constraints": {
        "type": "boolean",
        "default": true
      }
    }
  ],
  "computedDefaults": {
    "authentication": "privateKey",
    "organization": "",
    "event": "",
    "actions": [],
    "resolveData": true
  },
  "conditionalFields": [],
  "fixedCollections": []
}