{
  "nodeType": "n8n-nodes-base.googleDriveTrigger",
  "displayName": "Google Drive Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "triggerOn"
  ],
  "optionalParameters": [
    "authentication",
    "fileToWatch",
    "event",
    "folderToWatch",
    "event",
    "asas",
    "driveToWatch",
    "event",
    "options"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "oAuth2",
        "enumValues": [
          "oAuth2",
          "serviceAccount"
        ]
      }
    },
    {
      "field": "triggerOn",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": "",
        "enumValues": [
          "specificFile",
          "specificFolder"
        ]
      }
    },
    {
      "field": "fileToWatch",
      "type": "resourceLocator",
      "required": true,
      "constraints": {
        "type": "resourceLocator",
        "default": {
          "mode": "list",
          "value": ""
        }
      },
      "displayOptions": {
        "show": {
          "triggerOn": [
            "specificFile"
          ]
        }
      },
      "dependsOn": [
        "triggerOn"
      ]
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": "fileUpdated",
        "enumValues": [
          "fileUpdated"
        ]
      },
      "displayOptions": {
        "show": {
          "triggerOn": [
            "specificFile"
          ]
        }
      },
      "dependsOn": [
        "triggerOn"
      ]
    },
    {
      "field": "folderToWatch",
      "type": "resourceLocator",
      "required": true,
      "constraints": {
        "type": "resourceLocator",
        "default": {
          "mode": "list",
          "value": ""
        }
      },
      "displayOptions": {
        "show": {
          "triggerOn": [
            "specificFolder"
          ]
        }
      },
      "dependsOn": [
        "triggerOn"
      ]
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": "",
        "enumValues": [
          "fileCreated",
          "fileUpdated",
          "folderCreated",
          "folderUpdated",
          "watchFolderUpdated"
        ]
      },
      "displayOptions": {
        "show": {
          "triggerOn": [
            "specificFolder"
          ]
        }
      },
      "dependsOn": [
        "triggerOn"
      ]
    },
    {
      "field": "asas",
      "type": "notice",
      "required": false,
      "constraints": {
        "type": "notice",
        "default": ""
      },
      "displayOptions": {
        "show": {
          "triggerOn": [
            "specificFolder"
          ]
        },
        "hide": {
          "event": [
            "watchFolderUpdated"
          ]
        }
      },
      "dependsOn": [
        "triggerOn",
        "event"
      ]
    },
    {
      "field": "driveToWatch",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": "root"
      },
      "displayOptions": {
        "show": {
          "triggerOn": [
            "anyFileFolder"
          ]
        }
      },
      "dependsOn": [
        "triggerOn"
      ]
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": "fileCreated",
        "enumValues": [
          "fileCreated",
          "fileUpdated",
          "folderCreated",
          "folderUpdated"
        ]
      },
      "displayOptions": {
        "show": {
          "triggerOn": [
            "anyFileFolder"
          ]
        }
      },
      "dependsOn": [
        "triggerOn"
      ]
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "constraints": {
        "type": "collection",
        "default": {},
        "properties": {},
        "requiredProperties": []
      },
      "displayOptions": {
        "show": {
          "event": [
            "fileCreated",
            "fileUpdated"
          ]
        },
        "hide": {
          "triggerOn": [
            "specificFile"
          ]
        }
      },
      "dependsOn": [
        "event",
        "triggerOn"
      ]
    }
  ],
  "computedDefaults": {
    "authentication": "oAuth2",
    "triggerOn": "",
    "fileToWatch": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "folderToWatch": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "asas": "",
    "driveToWatch": "root",
    "options": {}
  },
  "conditionalFields": [
    {
      "field": "fileToWatch",
      "condition": {
        "show": {
          "triggerOn": [
            "specificFile"
          ]
        }
      },
      "dependsOnFields": [
        "triggerOn"
      ]
    },
    {
      "field": "event",
      "condition": {
        "show": {
          "triggerOn": [
            "specificFile"
          ]
        }
      },
      "dependsOnFields": [
        "triggerOn"
      ]
    },
    {
      "field": "folderToWatch",
      "condition": {
        "show": {
          "triggerOn": [
            "specificFolder"
          ]
        }
      },
      "dependsOnFields": [
        "triggerOn"
      ]
    },
    {
      "field": "event",
      "condition": {
        "show": {
          "triggerOn": [
            "specificFolder"
          ]
        }
      },
      "dependsOnFields": [
        "triggerOn"
      ]
    },
    {
      "field": "asas",
      "condition": {
        "show": {
          "triggerOn": [
            "specificFolder"
          ]
        },
        "hide": {
          "event": [
            "watchFolderUpdated"
          ]
        }
      },
      "dependsOnFields": [
        "triggerOn",
        "event"
      ]
    },
    {
      "field": "driveToWatch",
      "condition": {
        "show": {
          "triggerOn": [
            "anyFileFolder"
          ]
        }
      },
      "dependsOnFields": [
        "triggerOn"
      ]
    },
    {
      "field": "event",
      "condition": {
        "show": {
          "triggerOn": [
            "anyFileFolder"
          ]
        }
      },
      "dependsOnFields": [
        "triggerOn"
      ]
    },
    {
      "field": "options",
      "condition": {
        "show": {
          "event": [
            "fileCreated",
            "fileUpdated"
          ]
        },
        "hide": {
          "triggerOn": [
            "specificFile"
          ]
        }
      },
      "dependsOnFields": [
        "event",
        "triggerOn"
      ]
    }
  ],
  "fixedCollections": []
}