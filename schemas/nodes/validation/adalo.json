{
  "nodeType": "n8n-nodes-base.adalo",
  "displayName": "Adalo",
  "version": 1,
  "rules": [
    {
      "field": "resource",
      "type": "options",
      "required": false,
      "default": "collection",
      "enumValues": [
        "collection"
      ]
    },
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "getAll",
      "enumValues": [
        "create",
        "delete",
        "get",
        "getAll",
        "update"
      ]
    },
    {
      "field": "collectionId",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "resource": [
            "collection"
          ]
        }
      }
    },
    {
      "field": "rowId",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "get",
            "delete",
            "update"
          ],
          "resource": [
            "collection"
          ]
        }
      }
    },
    {
      "field": "dataToSend",
      "type": "options",
      "required": false,
      "default": "defineBelow",
      "enumValues": [
        "autoMapInputData",
        "defineBelow"
      ],
      "displayOptions": {
        "show": {
          "operation": [
            "create",
            "update"
          ],
          "resource": [
            "collection"
          ]
        }
      }
    },
    {
      "field": "inputsToIgnore",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "create",
            "update"
          ],
          "dataToSend": [
            "autoMapInputData"
          ],
          "resource": [
            "collection"
          ]
        }
      }
    },
    {
      "field": "fieldsUi",
      "type": "fixedCollection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "operation": [
            "create",
            "update"
          ],
          "dataToSend": [
            "defineBelow"
          ],
          "resource": [
            "collection"
          ]
        }
      }
    },
    {
      "field": "returnAll",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ],
          "resource": [
            "collection"
          ]
        }
      }
    },
    {
      "field": "limit",
      "type": "number",
      "required": false,
      "default": 100,
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ],
          "resource": [
            "collection"
          ],
          "returnAll": [
            false
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "resource": "collection",
    "operation": "getAll",
    "collectionId": "",
    "rowId": "",
    "dataToSend": "defineBelow",
    "inputsToIgnore": "",
    "fieldsUi": {},
    "returnAll": false,
    "limit": 100
  },
  "hasFilter": false,
  "fixedCollections": [
    "fieldsUi"
  ]
}