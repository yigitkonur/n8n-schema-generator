{
  "nodeType": "n8n-nodes-base.sendInBlueTrigger",
  "displayName": "Brevo Trigger",
  "version": 1,
  "rules": [
    {
      "field": "type",
      "type": "options",
      "required": true,
      "default": "transactional",
      "enumValues": [
        "inbound",
        "marketing",
        "transactional"
      ]
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "default": [],
      "enumValues": [
        "blocked",
        "click",
        "deferred",
        "delivered",
        "hardBounce",
        "invalid",
        "spam",
        "opened",
        "request",
        "softBounce",
        "uniqueOpened",
        "unsubscribed"
      ],
      "displayOptions": {
        "show": {
          "type": [
            "transactional"
          ]
        }
      }
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "default": [],
      "enumValues": [
        "click",
        "delivered",
        "hardBounce",
        "listAddition",
        "opened",
        "softBounce",
        "spam",
        "unsubscribed"
      ],
      "displayOptions": {
        "show": {
          "type": [
            "marketing"
          ]
        }
      }
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "default": [],
      "enumValues": [
        "inboundEmailProcessed"
      ],
      "displayOptions": {
        "show": {
          "type": [
            "inbound"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "type": "transactional",
    "events": []
  },
  "hasFilter": false,
  "fixedCollections": []
}