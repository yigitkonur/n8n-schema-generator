{
  "nodeType": "n8n-nodes-base.moveBinaryData",
  "displayName": "Convert to/from binary data",
  "version": [
    1,
    1.1
  ],
  "rules": [
    {
      "field": "mode",
      "type": "options",
      "required": false,
      "default": "binaryToJson",
      "enumValues": [
        "binaryToJson",
        "jsonToBinary"
      ]
    },
    {
      "field": "setAllData",
      "type": "boolean",
      "required": false,
      "default": true,
      "displayOptions": {
        "show": {
          "mode": [
            "binaryToJson"
          ]
        }
      }
    },
    {
      "field": "sourceKey",
      "type": "string",
      "required": true,
      "default": "data",
      "displayOptions": {
        "show": {
          "mode": [
            "binaryToJson"
          ]
        }
      }
    },
    {
      "field": "destinationKey",
      "type": "string",
      "required": true,
      "default": "data",
      "displayOptions": {
        "show": {
          "mode": [
            "binaryToJson"
          ],
          "setAllData": [
            false
          ]
        }
      }
    },
    {
      "field": "convertAllData",
      "type": "boolean",
      "required": false,
      "default": true,
      "displayOptions": {
        "show": {
          "mode": [
            "jsonToBinary"
          ]
        }
      }
    },
    {
      "field": "sourceKey",
      "type": "string",
      "required": true,
      "default": "data",
      "displayOptions": {
        "show": {
          "convertAllData": [
            false
          ],
          "mode": [
            "jsonToBinary"
          ]
        }
      }
    },
    {
      "field": "destinationKey",
      "type": "string",
      "required": true,
      "default": "data",
      "displayOptions": {
        "show": {
          "mode": [
            "jsonToBinary"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "mode": "binaryToJson",
    "setAllData": true,
    "sourceKey": "data",
    "convertAllData": true,
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}