{
  "nodeType": "n8n-nodes-base.figmaTrigger",
  "displayName": "Figma Trigger (Beta)",
  "version": 1,
  "rules": [
    {
      "field": "teamId",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "triggerOn",
      "type": "options",
      "required": true,
      "default": "",
      "enumValues": [
        "fileComment",
        "fileDelete",
        "fileUpdate",
        "fileVersionUpdate",
        "libraryPublish"
      ]
    }
  ],
  "computedDefaults": {
    "teamId": "",
    "triggerOn": ""
  },
  "hasFilter": false,
  "fixedCollections": []
}