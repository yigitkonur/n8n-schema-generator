{
  "nodeType": "n8n-nodes-base.openWeatherMap",
  "displayName": "OpenWeatherMap",
  "version": 1,
  "rules": [
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "currentWeather",
      "enumValues": [
        "currentWeather",
        "5DayForecast"
      ]
    },
    {
      "field": "format",
      "type": "options",
      "required": false,
      "default": "metric",
      "enumValues": [
        "imperial",
        "metric",
        "standard"
      ]
    },
    {
      "field": "locationSelection",
      "type": "options",
      "required": false,
      "default": "cityName",
      "enumValues": [
        "cityName",
        "cityId",
        "coordinates",
        "zipCode"
      ]
    },
    {
      "field": "cityName",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "locationSelection": [
            "cityName"
          ]
        }
      }
    },
    {
      "field": "cityId",
      "type": "number",
      "required": true,
      "default": 160001123,
      "displayOptions": {
        "show": {
          "locationSelection": [
            "cityId"
          ]
        }
      }
    },
    {
      "field": "latitude",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "locationSelection": [
            "coordinates"
          ]
        }
      }
    },
    {
      "field": "longitude",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "locationSelection": [
            "coordinates"
          ]
        }
      }
    },
    {
      "field": "zipCode",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "locationSelection": [
            "zipCode"
          ]
        }
      }
    },
    {
      "field": "language",
      "type": "string",
      "required": false,
      "default": ""
    }
  ],
  "computedDefaults": {
    "operation": "currentWeather",
    "format": "metric",
    "locationSelection": "cityName",
    "cityName": "",
    "cityId": 160001123,
    "latitude": "",
    "longitude": "",
    "zipCode": "",
    "language": ""
  },
  "hasFilter": false,
  "fixedCollections": []
}