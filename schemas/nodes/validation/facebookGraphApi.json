{
  "nodeType": "n8n-nodes-base.facebookGraphApi",
  "displayName": "Facebook Graph API",
  "version": 1,
  "rules": [
    {
      "field": "hostUrl",
      "type": "options",
      "required": true,
      "default": "graph.facebook.com",
      "enumValues": [
        "graph.facebook.com",
        "graph-video.facebook.com"
      ]
    },
    {
      "field": "httpRequestMethod",
      "type": "options",
      "required": true,
      "default": "GET",
      "enumValues": [
        "GET",
        "POST",
        "DELETE"
      ]
    },
    {
      "field": "graphApiVersion",
      "type": "options",
      "required": true,
      "default": "",
      "enumValues": [
        "",
        "v23.0",
        "v22.0",
        "v21.0",
        "v20.0",
        "v19.0",
        "v18.0",
        "v17.0",
        "v16.0",
        "v15.0",
        "v14.0",
        "v13.0",
        "v12.0",
        "v11.0",
        "v10.0",
        "v9.0",
        "v8.0",
        "v7.0",
        "v6.0",
        "v5.0",
        "v4.0",
        "v3.3",
        "v3.2",
        "v3.1",
        "v3.0"
      ]
    },
    {
      "field": "node",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "edge",
      "type": "string",
      "required": false,
      "default": ""
    },
    {
      "field": "allowUnauthorizedCerts",
      "type": "boolean",
      "required": false,
      "default": false
    },
    {
      "field": "sendBinaryData",
      "type": "boolean",
      "required": true,
      "default": false,
      "displayOptions": {
        "show": {
          "httpRequestMethod": [
            "POST",
            "PUT"
          ]
        }
      }
    },
    {
      "field": "binaryPropertyName",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "hide": {
          "sendBinaryData": [
            false
          ]
        },
        "show": {
          "httpRequestMethod": [
            "POST",
            "PUT"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "hostUrl": "graph.facebook.com",
    "httpRequestMethod": "GET",
    "graphApiVersion": "",
    "node": "",
    "edge": "",
    "allowUnauthorizedCerts": false,
    "sendBinaryData": false,
    "binaryPropertyName": "",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}