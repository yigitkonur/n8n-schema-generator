{
  "nodeType": "n8n-nodes-base.airtableTrigger",
  "displayName": "Airtable Trigger",
  "version": 1,
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "airtableApi",
      "enumValues": [
        "airtableApi",
        "airtableTokenApi",
        "airtableOAuth2Api"
      ]
    },
    {
      "field": "baseId",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "url",
        "value": ""
      }
    },
    {
      "field": "tableId",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "url",
        "value": ""
      }
    },
    {
      "field": "triggerField",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "downloadAttachments",
      "type": "boolean",
      "required": false,
      "default": false
    },
    {
      "field": "downloadFieldNames",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "downloadAttachments": [
            true
          ]
        }
      }
    },
    {
      "field": "additionalFields",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "authentication": "airtableApi",
    "baseId": {
      "__rl": true,
      "mode": "url",
      "value": ""
    },
    "tableId": {
      "__rl": true,
      "mode": "url",
      "value": ""
    },
    "triggerField": "",
    "downloadAttachments": false,
    "downloadFieldNames": "",
    "additionalFields": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}