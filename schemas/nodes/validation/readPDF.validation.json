{
  "nodeType": "n8n-nodes-base.readPDF",
  "displayName": "Read PDF",
  "version": 1,
  "category": [
    "input"
  ],
  "requiredParameters": [
    "binaryPropertyName",
    "encrypted"
  ],
  "optionalParameters": [
    "password"
  ],
  "validationRules": [
    {
      "field": "binaryPropertyName",
      "type": "string",
      "required": true,
      "constraints": {
        "type": "string",
        "default": "data"
      }
    },
    {
      "field": "encrypted",
      "type": "boolean",
      "required": true,
      "constraints": {
        "type": "boolean",
        "default": false
      }
    },
    {
      "field": "password",
      "type": "string",
      "required": false,
      "constraints": {
        "type": "string",
        "default": ""
      },
      "displayOptions": {
        "show": {
          "encrypted": [
            true
          ]
        }
      },
      "dependsOn": [
        "encrypted"
      ]
    }
  ],
  "computedDefaults": {
    "binaryPropertyName": "data",
    "encrypted": false,
    "password": ""
  },
  "conditionalFields": [
    {
      "field": "password",
      "condition": {
        "show": {
          "encrypted": [
            true
          ]
        }
      },
      "dependsOnFields": [
        "encrypted"
      ]
    }
  ],
  "fixedCollections": []
}