{
  "nodeType": "n8n-nodes-base.googleDriveTrigger",
  "displayName": "Google Drive Trigger",
  "version": 1,
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "oAuth2",
      "enumValues": [
        "oAuth2",
        "serviceAccount"
      ]
    },
    {
      "field": "triggerOn",
      "type": "options",
      "required": true,
      "default": "",
      "enumValues": [
        "specificFile",
        "specificFolder"
      ]
    },
    {
      "field": "fileToWatch",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "list",
        "value": ""
      },
      "displayOptions": {
        "show": {
          "triggerOn": [
            "specificFile"
          ]
        }
      }
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "default": "fileUpdated",
      "enumValues": [
        "fileUpdated"
      ],
      "displayOptions": {
        "show": {
          "triggerOn": [
            "specificFile"
          ]
        }
      }
    },
    {
      "field": "folderToWatch",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "list",
        "value": ""
      },
      "displayOptions": {
        "show": {
          "triggerOn": [
            "specificFolder"
          ]
        }
      }
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "default": "",
      "enumValues": [
        "fileCreated",
        "fileUpdated",
        "folderCreated",
        "folderUpdated",
        "watchFolderUpdated"
      ],
      "displayOptions": {
        "show": {
          "triggerOn": [
            "specificFolder"
          ]
        }
      }
    },
    {
      "field": "asas",
      "type": "notice",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "triggerOn": [
            "specificFolder"
          ]
        },
        "hide": {
          "event": [
            "watchFolderUpdated"
          ]
        }
      }
    },
    {
      "field": "driveToWatch",
      "type": "options",
      "required": true,
      "default": "root",
      "displayOptions": {
        "show": {
          "triggerOn": [
            "anyFileFolder"
          ]
        }
      }
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "default": "fileCreated",
      "enumValues": [
        "fileCreated",
        "fileUpdated",
        "folderCreated",
        "folderUpdated"
      ],
      "displayOptions": {
        "show": {
          "triggerOn": [
            "anyFileFolder"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "event": [
            "fileCreated",
            "fileUpdated"
          ]
        },
        "hide": {
          "triggerOn": [
            "specificFile"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "authentication": "oAuth2",
    "triggerOn": "",
    "fileToWatch": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "folderToWatch": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "asas": "",
    "driveToWatch": "root",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}