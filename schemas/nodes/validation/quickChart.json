{
  "nodeType": "n8n-nodes-base.quickChart",
  "displayName": "QuickChart",
  "version": 1,
  "rules": [
    {
      "field": "chartType",
      "type": "options",
      "required": false,
      "default": "bar",
      "enumValues": [
        "bar",
        "doughnut",
        "line",
        "pie",
        "polarArea"
      ]
    },
    {
      "field": "labelsMode",
      "type": "options",
      "required": false,
      "default": "manually",
      "enumValues": [
        "manually",
        "array"
      ]
    },
    {
      "field": "labelsUi",
      "type": "fixedCollection",
      "required": true,
      "default": {},
      "displayOptions": {
        "show": {
          "labelsMode": [
            "manually"
          ]
        }
      }
    },
    {
      "field": "labelsArray",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "labelsMode": [
            "array"
          ]
        }
      }
    },
    {
      "field": "data",
      "type": "json",
      "required": true,
      "default": ""
    },
    {
      "field": "output",
      "type": "string",
      "required": true,
      "default": "data"
    },
    {
      "field": "chartOptions",
      "type": "collection",
      "required": false,
      "default": {}
    },
    {
      "field": "datasetOptions",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "chartType": "bar",
    "labelsMode": "manually",
    "labelsUi": {},
    "labelsArray": "",
    "data": "",
    "output": "data",
    "chartOptions": {},
    "datasetOptions": {}
  },
  "hasFilter": false,
  "fixedCollections": [
    "labelsUi"
  ]
}