{
  "nodeType": "n8n-nodes-base.markdown",
  "displayName": "Markdown",
  "version": 1,
  "rules": [
    {
      "field": "mode",
      "type": "options",
      "required": false,
      "default": "htmlToMarkdown",
      "enumValues": [
        "markdownToHtml",
        "htmlToMarkdown"
      ]
    },
    {
      "field": "html",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "mode": [
            "htmlToMarkdown"
          ]
        }
      }
    },
    {
      "field": "markdown",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "mode": [
            "markdownToHtml"
          ]
        }
      }
    },
    {
      "field": "destinationKey",
      "type": "string",
      "required": true,
      "default": "data",
      "displayOptions": {
        "show": {
          "mode": [
            "markdownToHtml",
            "htmlToMarkdown"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "mode": [
            "htmlToMarkdown"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "mode": [
            "markdownToHtml"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "mode": "htmlToMarkdown",
    "html": "",
    "markdown": "",
    "destinationKey": "data",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}