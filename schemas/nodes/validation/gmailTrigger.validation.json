{
  "nodeType": "n8n-nodes-base.gmailTrigger",
  "displayName": "Gmail Trigger",
  "version": [
    1,
    1.1,
    1.2,
    1.3
  ],
  "category": [
    "trigger"
  ],
  "requiredParameters": [],
  "optionalParameters": [
    "authentication",
    "event",
    "simple",
    "filters",
    "options"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "oAuth2",
        "enumValues": [
          "oAuth2",
          "serviceAccount"
        ]
      }
    },
    {
      "field": "event",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "messageReceived",
        "enumValues": [
          "messageReceived"
        ]
      }
    },
    {
      "field": "simple",
      "type": "boolean",
      "required": false,
      "constraints": {
        "type": "boolean",
        "default": true
      }
    },
    {
      "field": "filters",
      "type": "collection",
      "required": false,
      "constraints": {
        "type": "collection",
        "default": {},
        "properties": {},
        "requiredProperties": []
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "constraints": {
        "type": "collection",
        "default": {},
        "properties": {},
        "requiredProperties": []
      },
      "displayOptions": {
        "hide": {
          "simple": [
            true
          ]
        }
      },
      "dependsOn": [
        "simple"
      ]
    }
  ],
  "computedDefaults": {
    "authentication": "oAuth2",
    "event": "messageReceived",
    "simple": true,
    "filters": {},
    "options": {}
  },
  "conditionalFields": [
    {
      "field": "options",
      "condition": {
        "hide": {
          "simple": [
            true
          ]
        }
      },
      "dependsOnFields": [
        "simple"
      ]
    }
  ],
  "fixedCollections": []
}