{
  "nodeType": "n8n-nodes-base.mauticTrigger",
  "displayName": "Mautic Trigger",
  "version": 1,
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "credentials",
      "enumValues": [
        "credentials",
        "oAuth2"
      ]
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "default": []
    },
    {
      "field": "eventsOrder",
      "type": "options",
      "required": false,
      "default": "ASC",
      "enumValues": [
        "ASC",
        "DESC"
      ]
    }
  ],
  "computedDefaults": {
    "authentication": "credentials",
    "events": [],
    "eventsOrder": "ASC"
  },
  "hasFilter": false,
  "fixedCollections": []
}