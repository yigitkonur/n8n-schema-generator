{
  "nodeType": "n8n-nodes-base.gitlabTrigger",
  "displayName": "GitLab Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "owner",
    "repository",
    "events"
  ],
  "optionalParameters": [
    "authentication"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "accessToken",
        "enumValues": [
          "accessToken",
          "oAuth2"
        ]
      }
    },
    {
      "field": "owner",
      "type": "string",
      "required": true,
      "constraints": {
        "type": "string",
        "default": ""
      }
    },
    {
      "field": "repository",
      "type": "string",
      "required": true,
      "constraints": {
        "type": "string",
        "default": ""
      }
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "constraints": {
        "type": "multiOptions",
        "default": [],
        "enumValues": [
          "note",
          "confidential_issues",
          "confidential_note",
          "deployment",
          "issues",
          "job",
          "merge_requests",
          "pipeline",
          "push",
          "releases",
          "tag_push",
          "wiki_page",
          "*"
        ]
      }
    }
  ],
  "computedDefaults": {
    "authentication": "accessToken",
    "owner": "",
    "repository": "",
    "events": []
  },
  "conditionalFields": [],
  "fixedCollections": []
}