{
  "nodeType": "n8n-nodes-base.interval",
  "displayName": "Interval",
  "version": 1,
  "category": [
    "trigger",
    "schedule"
  ],
  "requiredParameters": [],
  "optionalParameters": [
    "notice",
    "interval",
    "unit"
  ],
  "validationRules": [
    {
      "field": "notice",
      "type": "notice",
      "required": false,
      "constraints": {
        "type": "notice",
        "default": ""
      }
    },
    {
      "field": "interval",
      "type": "number",
      "required": false,
      "constraints": {
        "type": "number",
        "default": 1,
        "minValue": 1
      }
    },
    {
      "field": "unit",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "seconds",
        "enumValues": [
          "seconds",
          "minutes",
          "hours"
        ]
      }
    }
  ],
  "computedDefaults": {
    "notice": "",
    "interval": 1,
    "unit": "seconds"
  },
  "conditionalFields": [],
  "fixedCollections": []
}