{
  "nodeType": "n8n-nodes-base.awsComprehend",
  "displayName": "AWS Comprehend",
  "version": 1,
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "iam",
      "enumValues": [
        "iam",
        "assumeRole"
      ]
    },
    {
      "field": "resource",
      "type": "options",
      "required": false,
      "default": "text",
      "enumValues": [
        "text"
      ]
    },
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "detectDominantLanguage",
      "enumValues": [
        "detectDominantLanguage",
        "detectEntities",
        "detectSentiment"
      ]
    },
    {
      "field": "languageCode",
      "type": "options",
      "required": false,
      "default": "en",
      "enumValues": [
        "ar",
        "zh",
        "zh-TW",
        "en",
        "fr",
        "de",
        "hi",
        "it",
        "ja",
        "ko",
        "pt",
        "es"
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "text"
          ],
          "operation": [
            "detectSentiment",
            "detectEntities"
          ]
        }
      }
    },
    {
      "field": "text",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "resource": [
            "text"
          ]
        }
      }
    },
    {
      "field": "simple",
      "type": "boolean",
      "required": false,
      "default": true,
      "displayOptions": {
        "show": {
          "resource": [
            "text"
          ],
          "operation": [
            "detectDominantLanguage"
          ]
        }
      }
    },
    {
      "field": "additionalFields",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "resource": [
            "text"
          ],
          "operation": [
            "detectEntities"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "authentication": "iam",
    "resource": "text",
    "operation": "detectDominantLanguage",
    "languageCode": "en",
    "text": "",
    "simple": true,
    "additionalFields": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}