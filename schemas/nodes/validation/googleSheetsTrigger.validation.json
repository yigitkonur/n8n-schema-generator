{
  "nodeType": "n8n-nodes-base.googleSheetsTrigger",
  "displayName": "Google Sheets Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "documentId",
    "sheetName",
    "event"
  ],
  "optionalParameters": [
    "authentication",
    "includeInOutput",
    "options"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "hidden",
      "required": false,
      "constraints": {
        "type": "hidden",
        "default": "triggerOAuth2"
      }
    },
    {
      "field": "documentId",
      "type": "resourceLocator",
      "required": true,
      "constraints": {
        "type": "resourceLocator",
        "default": {
          "mode": "list",
          "value": ""
        }
      }
    },
    {
      "field": "sheetName",
      "type": "resourceLocator",
      "required": true,
      "constraints": {
        "type": "resourceLocator",
        "default": {
          "mode": "list",
          "value": ""
        }
      }
    },
    {
      "field": "event",
      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": "anyUpdate",
        "enumValues": [
          "rowAdded",
          "rowUpdate",
          "anyUpdate"
        ]
      }
    },
    {
      "field": "includeInOutput",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "new",
        "enumValues": [
          "new",
          "old",
          "both"
        ]
      },
      "displayOptions": {
        "hide": {
          "event": [
            "rowAdded"
          ]
        }
      },
      "dependsOn": [
        "event"
      ]
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "constraints": {
        "type": "collection",
        "default": {},
        "properties": {},
        "requiredProperties": []
      }
    }
  ],
  "computedDefaults": {
    "authentication": "triggerOAuth2",
    "documentId": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "sheetName": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "event": "anyUpdate",
    "includeInOutput": "new",
    "options": {}
  },
  "conditionalFields": [
    {
      "field": "includeInOutput",
      "condition": {
        "hide": {
          "event": [
            "rowAdded"
          ]
        }
      },
      "dependsOnFields": [
        "event"
      ]
    }
  ],
  "fixedCollections": []
}