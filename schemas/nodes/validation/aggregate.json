{
  "nodeType": "n8n-nodes-base.aggregate",
  "displayName": "Aggregate",
  "version": 1,
  "rules": [
    {
      "field": "aggregate",
      "type": "options",
      "required": false,
      "default": "aggregateIndividualFields",
      "enumValues": [
        "aggregateIndividualFields",
        "aggregateAllItemData"
      ]
    },
    {
      "field": "fieldsToAggregate",
      "type": "fixedCollection",
      "required": false,
      "default": {
        "fieldToAggregate": [
          {
            "fieldToAggregate": "",
            "renameField": false
          }
        ]
      },
      "displayOptions": {
        "show": {
          "aggregate": [
            "aggregateIndividualFields"
          ]
        }
      }
    },
    {
      "field": "destinationFieldName",
      "type": "string",
      "required": false,
      "default": "data",
      "displayOptions": {
        "show": {
          "aggregate": [
            "aggregateAllItemData"
          ]
        }
      }
    },
    {
      "field": "include",
      "type": "options",
      "required": false,
      "default": "allFields",
      "enumValues": [
        "allFields",
        "specifiedFields",
        "allFieldsExcept"
      ],
      "displayOptions": {
        "show": {
          "aggregate": [
            "aggregateAllItemData"
          ]
        }
      }
    },
    {
      "field": "fieldsToExclude",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "aggregate": [
            "aggregateAllItemData"
          ],
          "include": [
            "allFieldsExcept"
          ]
        }
      }
    },
    {
      "field": "fieldsToInclude",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "aggregate": [
            "aggregateAllItemData"
          ],
          "include": [
            "specifiedFields"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "aggregate": "aggregateIndividualFields",
    "fieldsToAggregate": {
      "fieldToAggregate": [
        {
          "fieldToAggregate": "",
          "renameField": false,
          "outputFieldName": ""
        }
      ]
    },
    "destinationFieldName": "data",
    "include": "allFields",
    "fieldsToExclude": "",
    "fieldsToInclude": "",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": [
    "fieldsToAggregate"
  ]
}