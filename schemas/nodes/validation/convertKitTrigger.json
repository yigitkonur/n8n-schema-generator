{
  "nodeType": "n8n-nodes-base.convertKitTrigger",
  "displayName": "ConvertKit Trigger",
  "version": 1,
  "rules": [
    {
      "field": "event",
      "type": "options",
      "required": true,
      "default": "",
      "enumValues": [
        "formSubscribe",
        "linkClick",
        "productPurchase",
        "purchaseCreate",
        "courseComplete",
        "courseSubscribe",
        "subscriberActivate",
        "subscriberUnsubscribe",
        "tagAdd",
        "tagRemove"
      ]
    },
    {
      "field": "formId",
      "type": "options",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "event": [
            "formSubscribe"
          ]
        }
      }
    },
    {
      "field": "courseId",
      "type": "options",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "event": [
            "courseSubscribe",
            "courseComplete"
          ]
        }
      }
    },
    {
      "field": "link",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "event": [
            "linkClick"
          ]
        }
      }
    },
    {
      "field": "productId",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "event": [
            "productPurchase"
          ]
        }
      }
    },
    {
      "field": "tagId",
      "type": "options",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "event": [
            "tagAdd",
            "tagRemove"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "event": "",
    "formId": "",
    "courseId": "",
    "link": "",
    "productId": "",
    "tagId": ""
  },
  "hasFilter": false,
  "fixedCollections": []
}