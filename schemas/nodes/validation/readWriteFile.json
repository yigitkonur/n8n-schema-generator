{
  "nodeType": "n8n-nodes-base.readWriteFile",
  "displayName": "Read/Write Files from Disk",
  "version": 1,
  "rules": [
    {
      "field": "info",
      "type": "notice",
      "required": false,
      "default": ""
    },
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "read",
      "enumValues": [
        "read",
        "write"
      ]
    },
    {
      "field": "fileSelector",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "read"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "operation": [
            "read"
          ]
        }
      }
    },
    {
      "field": "fileName",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "write"
          ]
        }
      }
    },
    {
      "field": "dataPropertyName",
      "type": "string",
      "required": true,
      "default": "data",
      "displayOptions": {
        "show": {
          "operation": [
            "write"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "operation": [
            "write"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "info": "",
    "operation": "read",
    "fileSelector": "",
    "options": {},
    "fileName": "",
    "dataPropertyName": "data"
  },
  "hasFilter": false,
  "fixedCollections": []
}