{
  "nodeType": "n8n-nodes-base.n8nTrainingCustomerDatastore",
  "displayName": "Customer Datastore (n8n training)",
  "version": 1,
  "rules": [
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "getOnePerson",
      "enumValues": [
        "getOnePerson",
        "getAllPeople"
      ]
    },
    {
      "field": "returnAll",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "operation": [
            "getAllPeople"
          ]
        }
      }
    },
    {
      "field": "limit",
      "type": "number",
      "required": false,
      "default": 5,
      "displayOptions": {
        "show": {
          "operation": [
            "getAllPeople"
          ],
          "returnAll": [
            false
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "operation": "getOnePerson",
    "returnAll": false,
    "limit": 5
  },
  "hasFilter": false,
  "fixedCollections": []
}