{
  "nodeType": "n8n-nodes-base.zendeskTrigger",
  "displayName": "Zendesk Trigger",
  "version": 1,
  "rules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "default": "apiToken",
      "enumValues": [
        "apiToken",
        "oAuth2"
      ]
    },
    {
      "field": "service",
      "type": "options",
      "required": true,
      "default": "support",
      "enumValues": [
        "support"
      ]
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "service": [
            "support"
          ]
        }
      }
    },
    {
      "field": "conditions",
      "type": "fixedCollection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "service": [
            "support"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "authentication": "apiToken",
    "service": "support",
    "options": {},
    "conditions": {}
  },
  "hasFilter": false,
  "fixedCollections": [
    "conditions"
  ]
}