{
  "nodeType": "n8n-nodes-base.slackTrigger",
  "displayName": "Slack Trigger",
  "version": 1,
  "rules": [
    {
      "field": "authentication",
      "type": "hidden",
      "required": false,
      "default": "accessToken"
    },
    {
      "field": "notice",
      "type": "notice",
      "required": false,
      "default": ""
    },
    {
      "field": "trigger",
      "type": "multiOptions",
      "required": false,
      "default": [],
      "enumValues": [
        "any_event",
        "app_mention",
        "file_public",
        "file_share",
        "message",
        "channel_created",
        "team_join",
        "reaction_added"
      ]
    },
    {
      "field": "watchWorkspace",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "trigger": [
            "any_event",
            "message",
            "reaction_added",
            "file_share",
            "app_mention"
          ]
        }
      }
    },
    {
      "field": "notice",
      "type": "notice",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "trigger": [
            "any_event",
            "message",
            "reaction_added",
            "file_share",
            "app_mention"
          ],
          "watchWorkspace": [
            true
          ]
        }
      }
    },
    {
      "field": "channelId",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "list",
        "value": ""
      },
      "displayOptions": {
        "show": {
          "watchWorkspace": [
            false
          ]
        }
      }
    },
    {
      "field": "downloadFiles",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "trigger": [
            "any_event",
            "file_share"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "authentication": "accessToken",
    "notice": "",
    "trigger": [],
    "watchWorkspace": false,
    "channelId": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "downloadFiles": false,
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}