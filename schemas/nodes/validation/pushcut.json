          "resource": [
            "notification"
          ],
          "operation": [
            "send"
          ]
        }
      }
    },
    {
      "field": "additionalFields",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "operation": [
            "send"
          ],
          "resource": [
            "notification"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "resource": "notification",
    "operation": "send",
    "notificationName": "",
    "additionalFields": {}
  },
  "hasFilter": false,
  "fixedCollections": []
