      "required": false,
      "default": "notification",
      "enumValues": [
        "notification"
      ]
    },
    {
      "field": "operation",
      "type": "options",
      "required": false,
      "default": "send",
      "enumValues": [
        "send"
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "notification"
          ]
        }
      }
    },
    {
      "field": "notificationName",
      "type": "options",
      "required": false,
      "default": "",
