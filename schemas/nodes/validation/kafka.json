{
  "nodeType": "n8n-nodes-base.kafka",
  "displayName": "Kafka",
  "version": 1,
  "rules": [
    {
      "field": "topic",
      "type": "string",
      "required": false,
      "default": ""
    },
    {
      "field": "sendInputData",
      "type": "boolean",
      "required": false,
      "default": true
    },
    {
      "field": "message",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "sendInputData": [
            false
          ]
        }
      }
    },
    {
      "field": "jsonParameters",
      "type": "boolean",
      "required": false,
      "default": false
    },
    {
      "field": "useSchemaRegistry",
      "type": "boolean",
      "required": false,
      "default": false
    },
    {
      "field": "schemaRegistryUrl",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "useSchemaRegistry": [
            true
          ]
        }
      }
    },
    {
      "field": "useKey",
      "type": "boolean",
      "required": false,
      "default": false
    },
    {
      "field": "key",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "useKey": [
            true
          ]
        }
      }
    },
    {
      "field": "eventName",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "useSchemaRegistry": [
            true
          ]
        }
      }
    },
    {
      "field": "headersUi",
      "type": "fixedCollection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "jsonParameters": [
            false
          ]
        }
      }
    },
    {
      "field": "headerParametersJson",
      "type": "json",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "jsonParameters": [
            true
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "topic": "",
    "sendInputData": true,
    "message": "",
    "jsonParameters": false,
    "useSchemaRegistry": false,
    "schemaRegistryUrl": "",
    "useKey": false,
    "key": "",
    "eventName": "",
    "headersUi": {},
    "headerParametersJson": "",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": [
    "headersUi"
  ]
}