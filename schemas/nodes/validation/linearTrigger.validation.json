{
  "nodeType": "n8n-nodes-base.linearTrigger",
  "displayName": "Linear Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "resources"
  ],
  "optionalParameters": [
    "authentication",
    "notice",
    "teamId"
  ],
  "validationRules": [
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "apiToken",
        "enumValues": [
          "apiToken",
          "oAuth2"
        ]
      }
    },
    {
      "field": "notice",
      "type": "notice",
      "required": false,
      "constraints": {
        "type": "notice",
        "default": ""
      }
    },
    {
      "field": "teamId",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": ""
      }
    },
    {
      "field": "resources",
      "type": "multiOptions",
      "required": true,
      "constraints": {
        "type": "multiOptions",
        "default": [],
        "enumValues": [
          "reaction",
          "cycle",
          "issue",
          "comment",
          "issueLabel",
          "project"
        ]
      }
    }
  ],
  "computedDefaults": {
    "authentication": "apiToken",
    "notice": "",
    "teamId": "",
    "resources": []
  },
  "conditionalFields": [],
  "fixedCollections": []
}