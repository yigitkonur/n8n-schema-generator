{
  "nodeType": "n8n-nodes-base.emeliaTrigger",
  "displayName": "Emelia Trigger",
  "version": 1,
  "rules": [
    {
      "field": "campaignId",
      "type": "options",
      "required": true,
      "default": ""
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "default": [],
      "enumValues": [
        "bounced",
        "opened",
        "replied",
        "sent",
        "clicked",
        "unsubscribed"
      ]
    }
  ],
  "computedDefaults": {
    "campaignId": "",
    "events": []
  },
  "hasFilter": false,
  "fixedCollections": []
}