{
  "nodeType": "n8n-nodes-base.googleFirebaseRealtimeDatabase",
  "displayName": "Google Cloud Realtime Database",
  "version": 1,
  "rules": [
    {
      "field": "projectId",
      "type": "options",
      "required": true,
      "default": ""
    },
    {
      "field": "operation",
      "type": "options",
      "required": true,
      "default": "create",
      "enumValues": [
        "create",
        "delete",
        "get",
        "push",
        "update"
      ]
    },
    {
      "field": "path",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "hide": {
          "operation": [
            "get"
          ]
        }
      }
    },
    {
      "field": "path",
      "type": "string",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "get"
          ]
        }
      }
    },
    {
      "field": "attributes",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "operation": [
            "create",
            "push",
            "update"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "projectId": "",
    "operation": "create",
    "path": "",
    "attributes": ""
  },
  "hasFilter": false,
  "fixedCollections": []
}