{
  "nodeType": "n8n-nodes-base.githubTrigger",
  "displayName": "Github Trigger",
  "version": 1,
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "owner",
    "repository",
    "events"
  ],
  "optionalParameters": [
    "notice",
    "authentication",
    "options"
  ],
  "validationRules": [
    {
      "field": "notice",
      "type": "notice",
      "required": false,
      "constraints": {
        "type": "notice",
        "default": ""
      }
    },
    {
      "field": "authentication",
      "type": "options",
      "required": false,
      "constraints": {
        "type": "options",
        "default": "accessToken",
        "enumValues": [
          "accessToken",
          "oAuth2"
        ]
      }
    },
    {
      "field": "owner",
      "type": "resourceLocator",
      "required": true,
      "constraints": {
        "type": "resourceLocator",
        "default": {
          "mode": "list",
          "value": ""
        }
      }
    },
    {
      "field": "repository",
      "type": "resourceLocator",
      "required": true,
      "constraints": {
        "type": "resourceLocator",
        "default": {
          "mode": "list",
          "value": ""
        }
      }
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "constraints": {
        "type": "multiOptions",
        "default": [],
        "enumValues": [
          "*",
          "check_run",
          "check_suite",
          "commit_comment",
          "create",
          "delete",
          "deploy_key",
          "deployment",
          "deployment_status",
          "fork",
          "github_app_authorization",
          "gollum",
          "installation",
          "installation_repositories",
          "issue_comment",
          "issues",
          "label",
          "marketplace_purchase",
          "member",
          "membership",
          "meta",
          "milestone",
          "org_block",
          "organization",
          "page_build",
          "project",
          "project_card",
          "project_column",
          "public",
          "pull_request",
          "pull_request_review",
          "pull_request_review_comment",
          "push",
          "release",
          "repository",
          "repository_import",
          "repository_vulnerability_alert",
          "security_advisory",
          "star",
          "status",
          "team",
          "team_add",
          "watch"
        ]
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "constraints": {
        "type": "collection",
        "default": {},
        "properties": {},
        "requiredProperties": []
      }
    }
  ],
  "computedDefaults": {
    "notice": "",
    "authentication": "accessToken",
    "owner": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "repository": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "events": [],
    "options": {}
  },
  "conditionalFields": [],
  "fixedCollections": []
}