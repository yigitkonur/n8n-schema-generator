{
  "nodeType": "n8n-nodes-base.kafkaTrigger",
  "displayName": "Kafka Trigger",
  "version": [
    1,
    1.1,
    1.2,
    1.3
  ],
  "rules": [
    {
      "field": "topic",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "groupId",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "resolveOffset",
      "type": "options",
      "required": false,
      "default": "onCompletion",
      "enumValues": [
        "onCompletion",
        "onSuccess",
        "onStatus",
        "immediately"
      ],
      "displayOptions": {
        "show": {
          "@version": [
            {
              "_cnd": {
                "gte": 1.3
              }
            }
          ]
        }
      }
    },
    {
      "field": "allowedStatuses",
      "type": "multiOptions",
      "required": false,
      "default": [
        "success"
      ],
      "enumValues": [
        "canceled",
        "crashed",
        "error",
        "new",
        "running",
        "success",
        "unknown",
        "waiting"
      ],
      "displayOptions": {
        "show": {
          "@version": [
            {
              "_cnd": {
                "gte": 1.3
              }
            }
          ],
          "resolveOffset": [
            "onStatus"
          ]
        }
      }
    },
    {
      "field": "useSchemaRegistry",
      "type": "boolean",
      "required": false,
      "default": false
    },
    {
      "field": "schemaRegistryUrl",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "useSchemaRegistry": [
            true
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "topic": "",
    "groupId": "",
    "resolveOffset": "onCompletion",
    "allowedStatuses": [
      "success"
    ],
    "useSchemaRegistry": false,
    "schemaRegistryUrl": "",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}