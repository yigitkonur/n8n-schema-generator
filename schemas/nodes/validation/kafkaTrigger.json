{
  "nodeType": "n8n-nodes-base.kafkaTrigger",
  "displayName": "Kafka Trigger",
  "version": [
    1,
    1.1
  ],
  "rules": [
    {
      "field": "topic",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "groupId",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "useSchemaRegistry",
      "type": "boolean",
      "required": false,
      "default": false
    },
    {
      "field": "schemaRegistryUrl",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "useSchemaRegistry": [
            true
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "topic": "",
    "groupId": "",
    "useSchemaRegistry": false,
    "schemaRegistryUrl": "",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}