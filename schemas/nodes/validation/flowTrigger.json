{
  "nodeType": "n8n-nodes-base.flowTrigger",
  "displayName": "Flow Trigger",
  "version": 1,
  "rules": [
    {
      "field": "resource",
      "type": "options",
      "required": false,
      "default": "",
      "enumValues": [
        "list",
        "task"
      ]
    },
    {
      "field": "listIds",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "resource": [
            "list"
          ]
        },
        "hide": {
          "resource": [
            "task"
          ]
        }
      }
    },
    {
      "field": "taskIds",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "resource": [
            "task"
          ]
        },
        "hide": {
          "resource": [
            "list"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "resource": "",
    "listIds": "",
    "taskIds": ""
  },
  "hasFilter": false,
  "fixedCollections": []
}