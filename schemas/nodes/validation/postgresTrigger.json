{
  "nodeType": "n8n-nodes-base.postgresTrigger",
  "displayName": "Postgres Trigger",
  "version": 1,
  "rules": [
    {
      "field": "triggerMode",
      "type": "options",
      "required": false,
      "default": "createTrigger",
      "enumValues": [
        "createTrigger",
        "listenTrigger"
      ]
    },
    {
      "field": "schema",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "list",
        "value": "public"
      },
      "displayOptions": {
        "show": {
          "triggerMode": [
            "createTrigger"
          ]
        }
      }
    },
    {
      "field": "tableName",
      "type": "resourceLocator",
      "required": true,
      "default": {
        "mode": "list",
        "value": ""
      },
      "displayOptions": {
        "show": {
          "triggerMode": [
            "createTrigger"
          ]
        }
      }
    },
    {
      "field": "channelName",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "triggerMode": [
            "listenTrigger"
          ]
        }
      }
    },
    {
      "field": "firesOn",
      "type": "options",
      "required": false,
      "default": "INSERT",
      "enumValues": [
        "INSERT",
        "UPDATE",
        "DELETE"
      ],
      "displayOptions": {
        "show": {
          "triggerMode": [
            "createTrigger"
          ]
        }
      }
    },
    {
      "field": "additionalFields",
      "type": "collection",
      "required": false,
      "default": {},
      "displayOptions": {
        "show": {
          "triggerMode": [
            "createTrigger"
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "triggerMode": "createTrigger",
    "schema": {
      "__rl": true,
      "mode": "list",
      "value": "public"
    },
    "tableName": {
      "__rl": true,
      "mode": "list",
      "value": ""
    },
    "channelName": "",
    "firesOn": "INSERT",
    "additionalFields": {},
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}