{
  "nodeType": "n8n-nodes-base.facebookTrigger",
  "displayName": "Facebook Trigger",
  "version": 1,
  "rules": [
    {
      "field": "appId",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "whatsappBusinessAccountNotice",
      "type": "notice",
      "required": false,
      "default": "",
      "displayOptions": {
        "show": {
          "object": [
            "whatsappBusinessAccount"
          ]
        }
      }
    },
    {
      "field": "object",
      "type": "options",
      "required": true,
      "default": "user",
      "enumValues": [
        "adAccount",
        "application",
        "certificateTransparency",
        "group",
        "instagram",
        "link",
        "page",
        "permissions",
        "user",
        "whatsappBusinessAccount",
        "workplaceSecurity"
      ]
    },
    {
      "field": "fields",
      "type": "multiOptions",
      "required": false,
      "default": []
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "appId": "",
    "whatsappBusinessAccountNotice": "",
    "object": "user",
    "fields": [],
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}