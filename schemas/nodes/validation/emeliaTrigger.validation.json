      "type": "options",
      "required": true,
      "constraints": {
        "type": "options",
        "default": ""
      }
    },
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "constraints": {
        "type": "multiOptions",
        "default": [],
        "enumValues": [
          "bounced",
          "opened",
          "replied",
          "sent",
          "clicked",
          "unsubscribed"
        ]
      }
    }
  ],
  "computedDefaults": {
    "campaignId": "",
    "events": []
  },
  "conditionalFields": [],
  "fixedCollections": []
}