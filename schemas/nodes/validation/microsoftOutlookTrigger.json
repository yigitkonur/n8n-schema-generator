{
  "nodeType": "n8n-nodes-base.microsoftOutlookTrigger",
  "displayName": "Microsoft Outlook Trigger",
  "version": 1,
  "rules": [
    {
      "field": "event",
      "type": "options",
      "required": false,
      "default": "messageReceived",
      "enumValues": [
        "messageReceived"
      ]
    },
    {
      "field": "output",
      "type": "options",
      "required": false,
      "default": "simple",
      "enumValues": [
        "simple",
        "raw",
        "fields"
      ]
    },
    {
      "field": "fields",
      "type": "multiOptions",
      "required": false,
      "default": [],
      "enumValues": [
        "bccRecipients",
        "body",
        "bodyPreview",
        "categories",
        "ccRecipients",
        "changeKey",
        "conversationId",
        "createdDateTime",
        "flag",
        "from",
        "hasAttachments",
        "importance",
        "inferenceClassification",
        "internetMessageId",
        "isDeliveryReceiptRequested",
        "isDraft",
        "isRead",
        "isReadReceiptRequested",
        "lastModifiedDateTime",
        "parentFolderId",
        "receivedDateTime",
        "replyTo",
        "sender",
        "sentDateTime",
        "subject",
        "toRecipients",
        "webLink"
      ],
      "displayOptions": {
        "show": {
          "output": [
            "fields"
          ]
        }
      }
    },
    {
      "field": "filters",
      "type": "collection",
      "required": false,
      "default": {}
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "event": "messageReceived",
    "output": "simple",
    "fields": [],
    "filters": {},
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}