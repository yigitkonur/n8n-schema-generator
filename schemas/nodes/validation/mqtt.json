{
  "nodeType": "n8n-nodes-base.mqtt",
  "displayName": "MQTT",
  "version": 1,
  "rules": [
    {
      "field": "topic",
      "type": "string",
      "required": true,
      "default": ""
    },
    {
      "field": "sendInputData",
      "type": "boolean",
      "required": false,
      "default": true
    },
    {
      "field": "message",
      "type": "string",
      "required": true,
      "default": "",
      "displayOptions": {
        "show": {
          "sendInputData": [
            false
          ]
        }
      }
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "default": {}
    }
  ],
  "computedDefaults": {
    "topic": "",
    "sendInputData": true,
    "message": "",
    "options": {}
  },
  "hasFilter": false,
  "fixedCollections": []
}