{
  "nodeType": "n8n-nodes-base.postmarkTrigger",
  "displayName": "Postmark Trigger",
  "version": 1,
  "rules": [
    {
      "field": "events",
      "type": "multiOptions",
      "required": true,
      "default": [],
      "enumValues": [
        "bounce",
        "click",
        "delivery",
        "open",
        "spamComplaint",
        "subscriptionChange"
      ]
    },
    {
      "field": "firstOpen",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "events": [
            "open"
          ]
        }
      }
    },
    {
      "field": "includeContent",
      "type": "boolean",
      "required": false,
      "default": false,
      "displayOptions": {
        "show": {
          "events": [
            "bounce",
            "spamComplaint"
          ]
        }
      }
    }
  ],
  "computedDefaults": {
    "events": [],
    "firstOpen": false,
    "includeContent": false
  },
  "hasFilter": false,
  "fixedCollections": []
}