{
  "nodeType": "n8n-nodes-base.kafkaTrigger",
  "displayName": "Kafka Trigger",
  "version": [
    1,
    1.1
  ],
  "category": [
    "trigger"
  ],
  "requiredParameters": [
    "topic",
    "groupId"
  ],
  "optionalParameters": [
    "useSchemaRegistry",
    "schemaRegistryUrl",
    "options"
  ],
  "validationRules": [
    {
      "field": "topic",
      "type": "string",
      "required": true,
      "constraints": {
        "type": "string",
        "default": ""
      }
    },
    {
      "field": "groupId",
      "type": "string",
      "required": true,
      "constraints": {
        "type": "string",
        "default": ""
      }
    },
    {
      "field": "useSchemaRegistry",
      "type": "boolean",
      "required": false,
      "constraints": {
        "type": "boolean",
        "default": false
      }
    },
    {
      "field": "schemaRegistryUrl",
      "type": "string",
      "required": true,
      "constraints": {
        "type": "string",
        "default": ""
      },
      "displayOptions": {
        "show": {
          "useSchemaRegistry": [
            true
          ]
        }
      },
      "dependsOn": [
        "useSchemaRegistry"
      ]
    },
    {
      "field": "options",
      "type": "collection",
      "required": false,
      "constraints": {
        "type": "collection",
        "default": {},
        "properties": {},
        "requiredProperties": []
      }
    }
  ],
  "computedDefaults": {
    "topic": "",
    "groupId": "",
    "useSchemaRegistry": false,
    "schemaRegistryUrl": "",
    "options": {}
  },
  "conditionalFields": [
    {
      "field": "schemaRegistryUrl",
      "condition": {
        "show": {
          "useSchemaRegistry": [
            true
          ]
        }
      },
      "dependsOnFields": [
        "useSchemaRegistry"
      ]
    }
  ],
  "fixedCollections": []
}