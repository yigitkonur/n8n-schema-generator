{
  "name": "kafka",
  "displayName": "Kafka",
  "description": "Sends messages to a Kafka topic",
  "group": [
    "transform"
  ],
  "version": 1,
  "defaults": {
    "topic": "",
    "sendInputData": true,
    "message": "",
    "jsonParameters": false,
    "useSchemaRegistry": false,
    "schemaRegistryUrl": "",
    "useKey": false,
    "key": "",
    "eventName": "",
    "headersUi": {},
    "headerParametersJson": "",
    "options": {}
  },
  "inputs": [
    "main"
  ],
  "outputs": [
    "main"
  ],
  "properties": [
    {
      "name": "topic",
      "displayName": "Topic",
      "type": "string",
      "default": "",
      "description": "Name of the queue of topic to publish to",
      "placeholder": "topic-name"
    },
    {
      "name": "sendInputData",
      "displayName": "Send Input Data",
      "type": "boolean",
      "default": true,
      "description": "Whether to send the data the node receives as JSON to Kafka"
    },
    {
      "name": "message",
      "displayName": "Message",
      "type": "string",
      "default": "",
      "description": "The message to be sent",
      "displayOptions": {
        "show": {
          "sendInputData": [
            false
          ]
        }
      }
    },
    {
      "name": "jsonParameters",
      "displayName": "JSON Parameters",
      "type": "boolean",
      "default": false
    },
    {
      "name": "useSchemaRegistry",
      "displayName": "Use Schema Registry",
      "type": "boolean",
      "default": false,
      "description": "Whether to use Confluent Schema Registry"
    },
    {
      "name": "schemaRegistryUrl",
      "displayName": "Schema Registry URL",
      "type": "string",
      "default": "",
      "required": true,
      "description": "URL of the schema registry",
      "displayOptions": {
        "show": {
          "useSchemaRegistry": [
            true
          ]
        }
      },
      "placeholder": "https://schema-registry-domain:8081"
    },
    {
      "name": "useKey",
      "displayName": "Use Key",
      "type": "boolean",
      "default": false,
      "description": "Whether to use a message key"
    },
    {
      "name": "key",
      "displayName": "Key",
      "type": "string",
      "default": "",
      "required": true,
      "description": "The message key",
      "displayOptions": {
        "show": {
          "useKey": [
            true
          ]
        }
      }
    },
    {
      "name": "eventName",
      "displayName": "Event Name",
      "type": "string",
      "default": "",
      "required": true,
      "description": "Namespace and Name of Schema in Schema Registry (namespace.name)",
      "displayOptions": {
        "show": {
          "useSchemaRegistry": [
            true
          ]
        }
      }
    },
    {
      "name": "headersUi",
      "displayName": "Headers",
      "type": "fixedCollection",
      "default": {},
      "options": [
        {
          "name": "headerValues",
          "displayName": "Header",
          "values": [
            {
              "displayName": "Key",
              "name": "key",
              "type": "string",
              "default": ""
            },
            {
              "displayName": "Value",
              "name": "value",
              "type": "string",
              "default": ""
            }
          ]
        }
      ],
      "typeOptions": {
        "multipleValues": true
      },
      "displayOptions": {
        "show": {
          "jsonParameters": [
            false
          ]
        }
      },
      "placeholder": "Add Header"
    },
    {
      "name": "headerParametersJson",
      "displayName": "Headers (JSON)",
      "type": "json",
      "default": "",
      "description": "Header parameters as JSON (flat object)",
      "displayOptions": {
        "show": {
          "jsonParameters": [
            true
          ]
        }
      }
    },
    {
      "name": "options",
      "displayName": "Options",
      "type": "collection",
      "default": {},
      "options": [
        {
          "displayName": "Acks",
          "name": "acks",
          "type": "boolean",
          "default": false,
          "description": "Whether or not producer must wait for acknowledgement from all replicas"
        },
        {
          "displayName": "Compression",
          "name": "compression",
          "type": "boolean",
          "default": false,
          "description": "Whether to send the data in a compressed format using the GZIP codec"
        },
        {
          "displayName": "Timeout",
          "name": "timeout",
          "type": "number",
          "default": 30000,
          "description": "The time to await a response in ms"
        }
      ],
      "placeholder": "Add option"
    }
  ],
  "credentials": [
    {
      "name": "kafka",
      "required": true,
      "testedBy": "kafkaConnectionTest"
    }
  ],
  "requiredFields": [
    "schemaRegistryUrl",
    "key",
    "eventName"
  ],
  "optionalFields": [
    "topic",
    "sendInputData",
    "message",
    "jsonParameters",
    "useSchemaRegistry",
    "useKey",
    "headersUi",
    "headerParametersJson",
    "options"
  ],
  "parameterTypes": {
    "topic": "string",
    "sendInputData": "boolean",
    "message": "string",
    "jsonParameters": "boolean",
    "useSchemaRegistry": "boolean",
    "schemaRegistryUrl": "string",
    "useKey": "boolean",
    "key": "string",
    "eventName": "string",
    "headersUi": "fixedCollection",
    "headerParametersJson": "json",
    "options": "collection"
  },
  "hasVersions": false
}