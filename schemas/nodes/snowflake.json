{
  "name": "snowflake",
  "displayName": "Snowflake",
  "description": "Get, add and update data in Snowflake",
  "group": [
    "input"
  ],
  "version": 1,
  "defaults": {
    "operation": "insert",
    "query": "",
    "table": "",
    "columns": "",
    "updateKey": "id"
  },
  "inputs": [
    "main"
  ],
  "outputs": [
    "main"
  ],
  "properties": [
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": "insert",
      "options": [
        {
          "name": "Execute Query",
          "value": "executeQuery",
          "description": "Execute an SQL query",
          "action": "Execute a SQL query"
        },
        {
          "name": "Insert",
          "value": "insert",
          "description": "Insert rows in database",
          "action": "Insert rows in database"
        },
        {
          "name": "Update",
          "value": "update",
          "description": "Update rows in database",
          "action": "Update rows in database"
        }
      ]
    },
    {
      "name": "query",
      "displayName": "Query",
      "type": "string",
      "default": "",
      "required": true,
      "description": "The SQL query to execute",
      "typeOptions": {
        "editor": "sqlEditor"
      },
      "displayOptions": {
        "show": {
          "operation": [
            "executeQuery"
          ]
        }
      }
    },
    {
      "name": "table",
      "displayName": "Table",
      "type": "string",
      "default": "",
      "required": true,
      "description": "Name of the table in which to insert data to",
      "displayOptions": {
        "show": {
          "operation": [
            "insert"
          ]
        }
      }
    },
    {
      "name": "columns",
      "displayName": "Columns",
      "type": "string",
      "default": "",
      "description": "Comma-separated list of the properties which should used as columns for the new rows",
      "displayOptions": {
        "show": {
          "operation": [
            "insert"
          ]
        }
      }
    },
    {
      "name": "table",
      "displayName": "Table",
      "type": "string",
      "default": "",
      "required": true,
      "description": "Name of the table in which to update data in",
      "displayOptions": {
        "show": {
          "operation": [
            "update"
          ]
        }
      }
    },
    {
      "name": "updateKey",
      "displayName": "Update Key",
      "type": "string",
      "default": "id",
      "required": true,
      "description": "Name of the property which decides which rows in the database should be updated. Normally that would be \"id\".",
      "displayOptions": {
        "show": {
          "operation": [
            "update"
          ]
        }
      }
    },
    {
      "name": "columns",
      "displayName": "Columns",
      "type": "string",
      "default": "",
      "description": "Comma-separated list of the properties which should used as columns for rows to update",
      "displayOptions": {
        "show": {
          "operation": [
            "update"
          ]
        }
      }
    }
  ],
  "credentials": [
    {
      "name": "snowflake",
      "required": true
    }
  ],
  "hasVersions": false
}