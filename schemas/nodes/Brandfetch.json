{
  "name": "Brandfetch",
  "displayName": "Brandfetch",
  "description": "Consume Brandfetch API",
  "group": [
    "output"
  ],
  "version": 1,
  "defaults": {
    "operation": "logo",
    "domain": "",
    "download": false,
    "imageTypes": [
      "logo",
      "icon"
    ],
    "imageFormats": [
      "png"
    ]
  },
  "inputs": [
    "main"
  ],
  "outputs": [
    "main"
  ],
  "properties": [
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": "logo",
      "options": [
        {
          "name": "Color",
          "value": "color",
          "description": "Return a company's colors",
          "action": "Return a company's colors"
        },
        {
          "name": "Company",
          "value": "company",
          "description": "Return a company's data",
          "action": "Return a company's data"
        },
        {
          "name": "Font",
          "value": "font",
          "description": "Return a company's fonts",
          "action": "Return a company's fonts"
        },
        {
          "name": "Industry",
          "value": "industry",
          "description": "Return a company's industry",
          "action": "Return a company's industry"
        },
        {
          "name": "Logo",
          "value": "logo",
          "description": "Return a company's logo & icon",
          "action": "Return a company's logo & icon"
        }
      ],
      "noDataExpression": true
    },
    {
      "name": "domain",
      "displayName": "Domain",
      "type": "string",
      "default": "",
      "required": true,
      "description": "The domain name of the company"
    },
    {
      "name": "download",
      "displayName": "Download",
      "type": "boolean",
      "default": false,
      "required": true,
      "description": "Name of the binary property to which to write the data of the read file",
      "displayOptions": {
        "show": {
          "operation": [
            "logo"
          ]
        }
      }
    },
    {
      "name": "imageTypes",
      "displayName": "Image Type",
      "type": "multiOptions",
      "default": [
        "logo",
        "icon"
      ],
      "required": true,
      "options": [
        {
          "name": "Icon",
          "value": "icon"
        },
        {
          "name": "Logo",
          "value": "logo"
        }
      ],
      "displayOptions": {
        "show": {
          "operation": [
            "logo"
          ],
          "download": [
            true
          ]
        }
      }
    },
    {
      "name": "imageFormats",
      "displayName": "Image Format",
      "type": "multiOptions",
      "default": [
        "png"
      ],
      "required": true,
      "description": "The image format in which the logo should be returned as",
      "options": [
        {
          "name": "PNG",
          "value": "png"
        },
        {
          "name": "SVG",
          "value": "svg"
        }
      ],
      "displayOptions": {
        "show": {
          "operation": [
            "logo"
          ],
          "download": [
            true
          ]
        }
      }
    }
  ],
  "credentials": [
    {
      "name": "brandfetchApi",
      "required": true
    }
  ],
  "requiredFields": [
    "domain",
    "download",
    "imageTypes",
    "imageFormats"
  ],
  "optionalFields": [
    "operation"
  ],
  "parameterTypes": {
    "operation": "options",
    "domain": "string",
    "download": "boolean",
    "imageTypes": "multiOptions",
    "imageFormats": "multiOptions"
  },
  "hasVersions": false
}