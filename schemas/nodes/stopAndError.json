{
  "name": "stopAndError",
  "displayName": "Stop and Error",
  "description": "Throw an error in the workflow",
  "group": [
    "input"
  ],
  "version": 1,
  "defaults": {
    "errorType": "errorMessage",
    "errorMessage": "",
    "errorObject": ""
  },
  "inputs": [
    "main"
  ],
  "outputs": [],
  "properties": [
    {
      "name": "errorType",
      "displayName": "Error Type",
      "type": "options",
      "default": "errorMessage",
      "description": "Type of error to throw",
      "options": [
        {
          "name": "Error Message",
          "value": "errorMessage"
        },
        {
          "name": "Error Object",
          "value": "errorObject"
        }
      ]
    },
    {
      "name": "errorMessage",
      "displayName": "Error Message",
      "type": "string",
      "default": "",
      "required": true,
      "displayOptions": {
        "show": {
          "errorType": [
            "errorMessage"
          ]
        }
      }
    },
    {
      "name": "errorObject",
      "displayName": "Error Object",
      "type": "json",
      "default": "",
      "required": true,
      "description": "Object containing error properties",
      "typeOptions": {
        "alwaysOpenEditWindow": true
      },
      "displayOptions": {
        "show": {
          "errorType": [
            "errorObject"
          ]
        }
      }
    }
  ],
  "credentials": [],
  "hasVersions": false
}