          "resource": [
            "databases"
          ],
          "operation": [
            "getAll"
          ]
        }
      }
    },
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": "getAll",
      "options": [
        {
          "name": "Get",
          "value": "get",
          "description": "Get specific alert",
          "routing": {
            "request": {
              "method": "GET",
              "url": "={{\"/api/alert/\" + $parameter.alertId}}"
            }
          },
          "action": "Get an alert"
        },
        {
          "name": "Get Many",
          "value": "getAll",
          "description": "Get many alerts",
          "routing": {
            "request": {
              "method": "GET",
              "url": "/api/alert/"
            }
          },
          "action": "Get many alerts"
        }
