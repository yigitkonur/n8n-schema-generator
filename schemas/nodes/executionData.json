{
  "name": "executionData",
  "displayName": "Execution Data",
  "description": "Add execution data for search",
  "group": [
    "input"
  ],
  "version": [
    1,
    1.1
  ],
  "defaults": {
    "notice": "",
    "operation": "save",
    "dataToSave": {}
  },
  "inputs": [
    "main"
  ],
  "outputs": [
    "main"
  ],
  "properties": [
    {
      "name": "notice",
      "displayName": "Save important data using this node. It will be displayed on each execution for easy reference and you can filter by it.<br />Filtering is available on Pro and Enterprise plans. <a href='https://n8n.io/pricing/' target='_blank'>More Info</a>",
      "type": "notice",
      "default": ""
    },
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": "save",
      "options": [
        {
          "name": "Save Highlight Data (for Search/review)",
          "value": "save",
          "action": "Save Highlight Data (for search/review)"
        }
      ],
      "noDataExpression": true
    },
    {
      "name": "dataToSave",
      "displayName": "Data to Save",
      "type": "fixedCollection",
      "default": {},
      "options": [
        {
          "displayName": "Values",
          "name": "values",
          "values": [
            {
              "displayName": "Key",
              "name": "key",
              "type": "string",
              "default": "",
              "placeholder": "e.g. myKey",
              "requiresDataPath": "single"
            },
            {
              "displayName": "Value",
              "name": "value",
              "type": "string",
              "default": "",
              "placeholder": "e.g. myValue"
            }
          ]
        }
      ],
      "typeOptions": {
        "multipleValueButtonText": "Add Saved Field",
        "multipleValues": true
      },
      "displayOptions": {
        "show": {
          "operation": [
            "save"
          ]
        }
      },
      "placeholder": "Add Saved Field"
    }
  ],
  "credentials": [],
  "requiredFields": [],
  "optionalFields": [
    "notice",
    "operation",
    "dataToSave"
  ],
  "parameterTypes": {
    "notice": "notice",
    "operation": "options",
    "dataToSave": "fixedCollection"
  },
  "hasVersions": false
}