{
  "name": "mailjetTrigger",
  "displayName": "Mailjet Trigger",
  "description": "Handle Mailjet events via webhooks",
  "group": [
    "trigger"
  ],
  "version": 1,
  "defaults": {
    "event": "open"
  },
  "inputs": [],
  "outputs": [
    "main"
  ],
  "properties": [
    {
      "name": "event",
      "displayName": "Event",
      "type": "options",
      "default": "open",
      "required": true,
      "description": "Determines which resource events the webhook is triggered for",
      "options": [
        {
          "name": "email.blocked",
          "value": "blocked"
        },
        {
          "name": "email.bounce",
          "value": "bounce"
        },
        {
          "name": "email.open",
          "value": "open"
        },
        {
          "name": "email.sent",
          "value": "sent"
        },
        {
          "name": "email.spam",
          "value": "spam"
        },
        {
          "name": "email.unsub",
          "value": "unsub"
        }
      ]
    }
  ],
  "credentials": [
    {
      "name": "mailjetEmailApi",
      "required": true
    }
  ],
  "requiredFields": [
    "event"
  ],
  "optionalFields": [],
  "parameterTypes": {
    "event": "options"
  },
  "hasVersions": false
}