{
  "name": "interval",
  "displayName": "Interval",
  "description": "Triggers the workflow in a given interval",
  "group": [
    "trigger",
    "schedule"
  ],
  "version": 1,
  "defaults": {
    "notice": "",
    "interval": 1,
    "unit": "seconds"
  },
  "inputs": [],
  "outputs": [
    "main"
  ],
  "properties": [
    {
      "name": "notice",
      "displayName": "This workflow will run on the schedule you define here once you <a data-key=\"activate\">activate</a> it.<br><br>For testing, you can also trigger it manually: by going back to the canvas and clicking 'execute workflow'",
      "type": "notice",
      "default": ""
    },
    {
      "name": "interval",
      "displayName": "Interval",
      "type": "number",
      "default": 1,
      "description": "Interval value",
      "typeOptions": {
        "minValue": 1
      }
    },
    {
      "name": "unit",
      "displayName": "Unit",
      "type": "options",
      "default": "seconds",
      "description": "Unit of the interval value",
      "options": [
        {
          "name": "Seconds",
          "value": "seconds"
        },
        {
          "name": "Minutes",
          "value": "minutes"
        },
        {
          "name": "Hours",
          "value": "hours"
        }
      ]
    }
  ],
  "credentials": [],
  "hasVersions": false
}