{
  "name": "nextCloudApi",
  "displayName": "NextCloud API",
  "documentationUrl": "nextcloud",
  "properties": [
    {
      "displayName": "Web DAV URL",
      "name": "webDavUrl",
      "type": "string",
      "placeholder": "https://nextcloud.example.com/remote.php/webdav",
      "default": ""
    },
    {
      "displayName": "User",
      "name": "user",
      "type": "string",
      "default": ""
    },
    {
      "displayName": "Password",
      "name": "password",
      "type": "string",
      "typeOptions": {
        "password": true
      },
      "default": ""
    }
  ],
  "authenticate": "custom"
}