{
  "name": "mistApi",
  "displayName": "Mist API",
  "documentationUrl": "mist",
  "properties": [
    {
      "displayName": "API Token",
      "name": "token",
      "type": "string",
      "typeOptions": {
        "password": true
      },
      "required": true,
      "default": ""
    },
    {
      "displayName": "Region",
      "name": "region",
      "type": "options",
      "options": [
        {
          "name": "Europe",
          "value": "eu"
        },
        {
          "name": "Global",
          "value": "global"
        }
      ],
      "default": "eu"
    }
  ],
  "authenticate": "custom"
}