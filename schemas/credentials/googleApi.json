      "default": "us-central1"
    },
    {
      "name": "email",
      "type": "string",
      "default": "",
      "required": true
    },
    {
      "name": "privateKey",
      "type": "string",
      "default": "",
      "required": true
    },
    {
      "name": "inpersonate",
      "type": "boolean",
      "default": false
    },
    {
      "name": "delegatedEmail",
      "type": "string",
      "default": ""
    },
    {
      "name": "httpNode",
      "type": "boolean",
      "default": false
    },
    {
      "name": "httpWarning",
      "type": "notice",
      "default": ""
    },
    {
      "name": "scopes",
      "type": "string",
      "default": ""
    }
  ]
}