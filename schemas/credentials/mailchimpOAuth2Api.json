{
  "name": "mailchimpOAuth2Api",
  "displayName": "Mailchimp OAuth2 API",
  "properties": [
    {
      "name": "grantType",
      "type": "hidden",
      "default": "authorizationCode"
    },
    {
      "name": "authUrl",
      "type": "hidden",
      "default": "https://login.mailchimp.com/oauth2/authorize",
      "required": true
    },
    {
      "name": "accessTokenUrl",
      "type": "hidden",
      "default": "https://login.mailchimp.com/oauth2/token",
      "required": true
    },
    {
      "name": "metadataUrl",
      "type": "hidden",
      "default": "https://login.mailchimp.com/oauth2/metadata",
      "required": true
    },
    {
      "name": "scope",
      "type": "hidden",
      "default": ""
    },
    {
      "name": "authQueryParameters",
      "type": "hidden",
      "default": ""
    },
    {
      "name": "authentication",
      "type": "hidden",
      "default": "body"
    }
  ]
}