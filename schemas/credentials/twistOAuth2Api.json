{
  "name": "twistOAuth2Api",
  "displayName": "Twist OAuth2 API",
  "properties": [
    {
      "name": "grantType",
      "type": "hidden",
      "default": "authorizationCode"
    },
    {
      "name": "authUrl",
      "type": "hidden",
      "default": "https://twist.com/oauth/authorize",
      "required": true
    },
    {
      "name": "accessTokenUrl",
      "type": "hidden",
      "default": "https://twist.com/oauth/access_token",
      "required": true
    },
    {
      "name": "scope",
      "type": "hidden",
      "default": "attachments:write,channels:remove,comments:remove,messages:remove,threads:remove,workspaces:read"
    },
    {
      "name": "authQueryParameters",
      "type": "hidden",
      "default": ""
    },
    {
      "name": "authentication",
      "type": "hidden",
      "default": "body"
    }
  ]
}