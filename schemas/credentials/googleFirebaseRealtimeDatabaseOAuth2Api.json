{
  "name": "googleFirebaseRealtimeDatabaseOAuth2Api",
  "displayName": "Google Firebase Realtime Database OAuth2 API",
  "documentationUrl": "google/oauth-single-service",
  "properties": [
    {
      "displayName": "Scope",
      "name": "scope",
      "type": "hidden",
      "default": "https://www.googleapis.com/auth/userinfo.email https://www.googleapis.com/auth/firebase.database https://www.googleapis.com/auth/firebase"
    },
    {
      "displayName": "Region",
      "name": "region",
      "type": "options",
      "default": "firebaseio.com",
      "options": [
        {
          "name": "us-central1",
          "value": "firebaseio.com"
        },
        {
          "name": "europe-west1",
          "value": "europe-west1.firebasedatabase.app"
        },
        {
          "name": "asia-southeast1",
          "value": "asia-southeast1.firebasedatabase.app"
        }
      ]
    }
  ]
}