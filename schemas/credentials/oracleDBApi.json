{
  "name": "oracleDBApi",
  "displayName": "Oracle Database Credentials API",
  "properties": [
    {
      "name": "user",
      "type": "string",
      "default": ""
    },
    {
      "name": "password",
      "type": "string",
      "default": ""
    },
    {
      "name": "connectionString",
      "type": "string",
      "default": "localhost/orcl"
    },
    {
      "name": "useThickMode",
      "type": "boolean",
      "default": false
    },
    {
      "name": "useSSL",
      "type": "boolean",
      "default": false
    },
    {
      "name": "walletPassword",
      "type": "string",
      "default": ""
    },
    {
      "name": "walletContent",
      "type": "string",
      "default": ""
    },
    {
      "name": "sslServerCertDN",
      "type": "string",
      "default": ""
    },
    {
      "name": "sslServerDNMatch",
      "type": "boolean",
      "default": true
    },
    {
      "name": "sslAllowWeakDNMatch",
      "type": "boolean",
      "default": false
    },
    {
      "name": "poolMin",
      "type": "number",
      "default": 0
    },
    {
      "name": "poolMax",
      "type": "number",
      "default": 4
    },
    {
      "name": "poolIncrement",
      "type": "number",
      "default": 1
    },
    {
      "name": "maxLifetimeSession",
      "type": "number",
      "default": 0
    },
    {
      "name": "poolTimeout",
      "type": "number",
      "default": 60
    },
    {
      "name": "connectionClass",
      "type": "string",
      "default": ""
    },
    {
      "name": "connectTimeout",
      "type": "number",
      "default": 0
    },
    {
      "name": "transportConnectTimeout",
      "type": "number",
      "default": 20
    },
    {
      "name": "expireTime",
      "type": "number",
      "default": 0
    }
  ]
}