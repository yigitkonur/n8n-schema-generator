{
  "name": "dropboxApi",
  "displayName": "Dropbox API",
  "documentationUrl": "dropbox",
  "properties": [
    {
      "displayName": "Access Token",
      "name": "accessToken",
      "type": "string",
      "typeOptions": {
        "password": true
      },
      "default": ""
    },
    {
      "displayName": "APP Access Type",
      "name": "accessType",
      "type": "options",
      "options": [
        {
          "name": "App Folder",
          "value": "folder"
        },
        {
          "name": "Full Dropbox",
          "value": "full"
        }
      ],
      "default": "full"
    }
  ],
  "authenticate": "custom"
}