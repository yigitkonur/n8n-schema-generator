{
  "name": "azureStorageOAuth2Api",
  "displayName": "Azure Storage OAuth2 API",
  "documentationUrl": "azurestorage",
  "properties": [
    {
      "displayName": "Account",
      "name": "account",
      "type": "string",
      "default": ""
    },
    {
      "displayName": "Base URL",
      "name": "baseUrl",
      "type": "hidden",
      "default": "=https://{{ $self[\"account\"] }}.blob.core.windows.net"
    },
    {
      "displayName": "Scope",
      "name": "scope",
      "type": "hidden",
      "default": "https://storage.azure.com/.default"
    }
  ]
}