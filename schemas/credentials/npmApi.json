{
  "name": "npmApi",
  "displayName": "Npm API",
  "documentationUrl": "npm",
  "properties": [
    {
      "displayName": "Access Token",
      "name": "accessToken",
      "type": "string",
      "typeOptions": {
        "password": true
      },
      "default": ""
    },
    {
      "displayName": "Registry Url",
      "name": "registryUrl",
      "type": "string",
      "default": "https://registry.npmjs.org"
    }
  ],
  "authenticate": "custom"
}