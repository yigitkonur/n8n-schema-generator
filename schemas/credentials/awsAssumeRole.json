{
  "name": "awsAssumeRole",
  "displayName": "AWS (Assume Role)",
  "properties": [
    {
      "name": "region",
      "type": "options",
      "default": "us-east-1"
    },
    {
      "name": "useSystemCredentialsForRole",
      "type": "boolean",
      "default": false
    },
    {
      "name": "stsAccessKeyId",
      "type": "string",
      "default": "",
      "required": true
    },
    {
      "name": "stsSecretAccessKey",
      "type": "string",
      "default": "",
      "required": true
    },
    {
      "name": "stsSessionToken",
      "type": "string",
      "default": ""
    },
    {
      "name": "roleArn",
      "type": "string",
      "default": "",
      "required": true
    },
    {
      "name": "externalId",
      "type": "string",
      "default": "",
      "required": true
    },
    {
      "name": "roleSessionName",
      "type": "string",
      "default": "n8n-session",
      "required": true
    },
    {
      "name": "customEndpoints",
      "type": "boolean",
      "default": false
