      "type": "string",
      "default": ""
    },
    {
      "name": "sesEndpoint",
      "type": "string",
      "default": ""
    },
    {
      "name": "sqsEndpoint",
      "type": "string",
      "default": ""
    },
    {
      "name": "s3Endpoint",
      "type": "string",
      "default": ""
    },
    {
      "name": "ssmEndpoint",
      "type": "string",
      "default": ""
    }
  ]
}