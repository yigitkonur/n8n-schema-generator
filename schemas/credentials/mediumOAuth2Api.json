{
  "name": "mediumOAuth2Api",
  "displayName": "Medium OAuth2 API",
  "properties": [
    {
      "name": "grantType",
      "type": "hidden",
      "default": "authorizationCode"
    },
    {
      "name": "authUrl",
      "type": "hidden",
      "default": "https://medium.com/m/oauth/authorize",
      "required": true
    },
    {
      "name": "accessTokenUrl",
      "type": "hidden",
      "default": "https://medium.com/v1/tokens",
      "required": true
    },
    {
      "name": "scope",
      "type": "hidden",
      "default": "basicProfile,publishPost,listPublications"
    },
    {
      "name": "clientId",
      "type": "string",
      "default": "",
      "required": true
    },
    {
      "name": "clientSecret",
      "type": "string",
      "default": "",
      "required": true
    },
    {
      "name": "authQueryParameters",
      "type": "hidden",
      "default": ""
    },
    {
      "name": "authentication",
      "type": "hidden",
      "default": "body"
    }
  ]
}