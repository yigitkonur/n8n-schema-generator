{
  "name": "datadogApi",
  "displayName": "Datadog API",
  "documentationUrl": "datadog",
  "properties": [
    {
      "displayName": "URL",
      "name": "url",
      "required": true,
      "type": "string",
      "default": "https://api.datadoghq.com"
    },
    {
      "displayName": "API Key",
      "name": "apiKey",
      "required": true,
      "type": "string",
      "typeOptions": {
        "password": true
      },
      "default": ""
    },
    {
      "displayName": "APP Key",
      "name": "appKey",
      "required": false,
      "type": "string",
      "default": "",
      "typeOptions": {
        "password": true
      },
      "description": "For some endpoints, you also need an Application key."
    }
  ],
  "authenticate": "custom"
}