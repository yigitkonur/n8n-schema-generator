{
  "name": "ldap",
  "displayName": "LDAP",
  "properties": [
    {
      "name": "hostname",
      "type": "string",
      "default": "",
      "required": true
    },
    {
      "name": "port",
      "type": "string",
      "default": "389"
    },
    {
      "name": "bindDN",
      "type": "string",
      "default": ""
    },
    {
      "name": "bindPassword",
      "type": "string",
      "default": ""
    },
    {
      "name": "connectionSecurity",
      "type": "options",
      "default": "none"
    },
    {
      "name": "allowUnauthorizedCerts",
      "type": "boolean",
      "default": false
    },
    {
      "name": "caCertificate",
      "type": "string",
      "default": ""
    },
    {
      "name": "timeout",
      "type": "number",
      "default": 300
    }
  ]
}