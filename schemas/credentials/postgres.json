{
  "name": "postgres",
  "displayName": "Postgres",
  "properties": [
    {
      "name": "host",
      "type": "string",
      "default": "localhost"
    },
    {
      "name": "database",
      "type": "string",
      "default": "postgres"
    },
    {
      "name": "user",
      "type": "string",
      "default": "postgres"
    },
    {
      "name": "password",
      "type": "string",
      "default": ""
    },
    {
      "name": "maxConnections",
      "type": "number",
      "default": 100
    },
    {
      "name": "allowUnauthorizedCerts",
      "type": "boolean",
      "default": false
    },
    {
      "name": "ssl",
      "type": "options",
      "default": "disable"
    },
    {
      "name": "port",
      "type": "number",
      "default": 5432
    },
    {
      "name": "sshTunnel",
      "type": "boolean",
      "default": false
    },
    {
      "name": "sshAuthenticateWith",
      "type": "options",
      "default": "password"
    },
    {
      "name": "sshHost",
      "type": "string",
      "default": "localhost"
    },
    {
      "name": "sshPort",
      "type": "number",
      "default": 22
    },
    {
      "name": "sshUser",
      "type": "string",
      "default": "root"
    },
    {
      "name": "sshPassword",
      "type": "string",
      "default": ""
    },
    {
      "name": "privateKey",
      "type": "string",
      "default": ""
    },
    {
      "name": "passphrase",
      "type": "string",
      "default": ""
    }
  ]
}